    {
        "BookCode":  "MAL",
        "SurahNumber":  "82",
        "AyahNumber":  "9",
        "Recitation":  "Alafasy_128kbps/082009",
        "Body":  "Nay! In fact you deny the Day of Judgement!"
    }
