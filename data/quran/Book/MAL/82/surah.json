[
    {
        "BookCode":  "MAL",
        "SurahNumber":  "82",
        "AyahNumber":  "1",
        "Recitation":  "Alafasy_128kbps/082001",
        "Body":  "When the heaven will cleft asunder;"
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "82",
        "AyahNumber":  "10",
        "Recitation":  "Alafasy_128kbps/082010",
        "Body":  "You should know that guardian angels have indeed been appointed\n\t\t\tover you,"
