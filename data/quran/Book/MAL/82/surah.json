    {
        "BookCode":  "MAL",
        "SurahNumber":  "82",
        "AyahNumber":  "3",
        "Recitation":  "Alafasy_128kbps/082003",
        "Body":  "when the oceans will be torn apart;"
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "82",
        "AyahNumber":  "4",
        "Recitation":  "Alafasy_128kbps/082004",
        "Body":  "and when the graves will be laid open:"
