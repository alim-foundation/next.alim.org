[
    {
        "BookCode":  "MAL",
        "SurahNumber":  "84",
        "AyahNumber":  "1",
        "Recitation":  "Alafasy_128kbps/084001",
        "Body":  "When heaven will split asunder,"
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "84",
        "AyahNumber":  "10",
        "Recitation":  "Alafasy_128kbps/084010",
        "Body":  "But he who will be given his book of deeds from behind his back,"
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "84",
        "AyahNumber":  "11",
        "Recitation":  "Alafasy_128kbps/084011",
        "Body":  "shall soon be calling for death,"
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "84",
        "AyahNumber":  "12",
        "Recitation":  "Alafasy_128kbps/084012",
        "Body":  "and shall be made to enter a blazing fire;"
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "84",
        "AyahNumber":  "13",
        "Recitation":  "Alafasy_128kbps/084013",
        "Body":  "for he used to live happily with his kinfolks"
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "84",
        "AyahNumber":  "14",
        "Recitation":  "Alafasy_128kbps/084014",
        "Body":  "and thought that he would never return to Allah for\n\t\t\taccountability."
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "84",
        "AyahNumber":  "15",
        "Recitation":  "Alafasy_128kbps/084015",
        "Body":  "Yet, surely his Rabb was ever watching over his misdeeds."
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "84",
        "AyahNumber":  "16",
        "Recitation":  "Alafasy_128kbps/084016",
        "Body":  "I swear by the glow of sunset;"
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "84",
        "AyahNumber":  "17",
        "Recitation":  "Alafasy_128kbps/084017",
        "Body":  "by the night and all that it gathers together;"
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "84",
        "AyahNumber":  "18",
        "Recitation":  "Alafasy_128kbps/084018",
        "Body":  "by the moon, when she grows full:"
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "84",
        "AyahNumber":  "19",
        "Recitation":  "Alafasy_128kbps/084019",
        "Body":  "that you shall gradually pass from one stage to another."
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "84",
        "AyahNumber":  "2",
        "Recitation":  "Alafasy_128kbps/084002",
        "Body":  "obeying her Rabb\u0027s command as she ought to."
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "84",
        "AyahNumber":  "20",
        "Recitation":  "Alafasy_128kbps/084020",
        "Body":  "What is the matter with the people that they do not believe,"
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "84",
        "AyahNumber":  "21",
        "Recitation":  "Alafasy_128kbps/084021",
        "Body":  "and when the Qur\u0027an is recited to them, they do not prostrate?"
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "84",
        "AyahNumber":  "22",
        "Recitation":  "Alafasy_128kbps/084022",
        "Body":  "Nay! The unbelievers will deny;"
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "84",
        "AyahNumber":  "23",
        "Recitation":  "Alafasy_128kbps/084023",
        "Body":  "and Allah knows what they are hiding in their hearts."
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "84",
        "AyahNumber":  "24",
        "Recitation":  "Alafasy_128kbps/084024",
        "Body":  "Therefore, proclaim to them a painful punishment,"
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "84",
        "AyahNumber":  "25",
        "Recitation":  "Alafasy_128kbps/084025",
        "Body":  "except those who embrace the true faith and do good deeds; for\n\t\t\tthem there will be a never ending reward."
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "84",
        "AyahNumber":  "3",
        "Recitation":  "Alafasy_128kbps/084003",
        "Body":  "When the earth will spread out"
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "84",
        "AyahNumber":  "4",
        "Recitation":  "Alafasy_128kbps/084004",
        "Body":  "and cast out all that is within her and becomes empty,"
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "84",
        "AyahNumber":  "5",
        "Recitation":  "Alafasy_128kbps/084005",
        "Body":  "obeying her Rabb\u0027s command as she ought to."
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "84",
        "AyahNumber":  "6",
        "Recitation":  "Alafasy_128kbps/084006",
        "Body":  "That will be the Day of your accountability, O mankind! Surely\n\t\t\tyou must strive towards your Rabb, a hard striving, then you\n\t\t\tshall meet Him."
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "84",
        "AyahNumber":  "7",
        "Recitation":  "Alafasy_128kbps/084007",
        "Body":  "Then he who will be given his Book of deeds in his right hand,"
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "84",
        "AyahNumber":  "8",
        "Recitation":  "Alafasy_128kbps/084008",
        "Body":  "shall have a quick and easy reckoning"
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "84",
        "AyahNumber":  "9",
        "Recitation":  "Alafasy_128kbps/084009",
        "Body":  "and shall return to his people rejoicing."
    }
]
