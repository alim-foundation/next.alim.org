        "AyahNumber":  "1",
        "Recitation":  "Alafasy_128kbps/087001",
        "Body":  "Glorify the name of your Rabb, the Most High,"
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "87",
        "AyahNumber":  "10",
        "Recitation":  "Alafasy_128kbps/087010",
