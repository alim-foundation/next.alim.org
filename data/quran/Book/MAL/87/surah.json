    {
        "BookCode":  "MAL",
        "SurahNumber":  "87",
        "AyahNumber":  "19",
        "Recitation":  "Alafasy_128kbps/087019",
        "Body":  "the scriptures of Ibrahim (Abraham) and Musa (Moses)."
    },
    {
        "BookCode":  "MAL",
        "SurahNumber":  "87",
        "AyahNumber":  "2",
        "Recitation":  "Alafasy_128kbps/087002",
        "Body":  "Who has created all things and perfected them."
