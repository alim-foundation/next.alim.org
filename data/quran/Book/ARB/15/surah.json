[
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "1",
        "Recitation":  "Alafasy_128kbps/015001",
        "Body":  "الٓر ۚ تِلْكَ ءَايَٰتُ ٱلْكِتَٰبِ وَقُرْءَانٍ مُّبِينٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "2",
        "Recitation":  "Alafasy_128kbps/015002",
        "Body":  "رُّبَمَا يَوَدُّ ٱلَّذِينَ كَفَرُوا۟ لَوْ كَانُوا۟ مُسْلِمِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "3",
        "Recitation":  "Alafasy_128kbps/015003",
        "Body":  "ذَرْهُمْ يَأْكُلُوا۟ وَيَتَمَتَّعُوا۟ وَيُلْهِهِمُ ٱلْأَمَلُ ۖ فَسَوْفَ يَعْلَمُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "4",
        "Recitation":  "Alafasy_128kbps/015004",
        "Body":  "وَمَآ أَهْلَكْنَا مِن قَرْيَةٍ إِلَّا وَلَهَا كِتَابٌ مَّعْلُومٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "5",
        "Recitation":  "Alafasy_128kbps/015005",
        "Body":  "مَّا تَسْبِقُ مِنْ أُمَّةٍ أَجَلَهَا وَمَا يَسْتَـْٔخِرُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "6",
        "Recitation":  "Alafasy_128kbps/015006",
        "Body":  "وَقَالُوا۟ يَٰٓأَيُّهَا ٱلَّذِى نُزِّلَ عَلَيْهِ ٱلذِّكْرُ إِنَّكَ لَمَجْنُونٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "7",
        "Recitation":  "Alafasy_128kbps/015007",
        "Body":  "لَّوْ مَا تَأْتِينَا بِٱلْمَلَٰٓئِكَةِ إِن كُنتَ مِنَ ٱلصَّٰدِقِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "8",
        "Recitation":  "Alafasy_128kbps/015008",
        "Body":  "مَا نُنَزِّلُ ٱلْمَلَٰٓئِكَةَ إِلَّا بِٱلْحَقِّ وَمَا كَانُوٓا۟ إِذًا مُّنظَرِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "9",
        "Recitation":  "Alafasy_128kbps/015009",
        "Body":  "إِنَّا نَحْنُ نَزَّلْنَا ٱلذِّكْرَ وَإِنَّا لَهُۥ لَحَٰفِظُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "10",
        "Recitation":  "Alafasy_128kbps/015010",
        "Body":  "وَلَقَدْ أَرْسَلْنَا مِن قَبْلِكَ فِى شِيَعِ ٱلْأَوَّلِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "11",
        "Recitation":  "Alafasy_128kbps/015011",
        "Body":  "وَمَا يَأْتِيهِم مِّن رَّسُولٍ إِلَّا كَانُوا۟ بِهِۦ يَسْتَهْزِءُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "12",
        "Recitation":  "Alafasy_128kbps/015012",
        "Body":  "كَذَٰلِكَ نَسْلُكُهُۥ فِى قُلُوبِ ٱلْمُجْرِمِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "13",
        "Recitation":  "Alafasy_128kbps/015013",
        "Body":  "لَا يُؤْمِنُونَ بِهِۦ ۖ وَقَدْ خَلَتْ سُنَّةُ ٱلْأَوَّلِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "14",
        "Recitation":  "Alafasy_128kbps/015014",
        "Body":  "وَلَوْ فَتَحْنَا عَلَيْهِم بَابًا مِّنَ ٱلسَّمَآءِ فَظَلُّوا۟ فِيهِ يَعْرُجُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "15",
        "Recitation":  "Alafasy_128kbps/015015",
        "Body":  "لَقَالُوٓا۟ إِنَّمَا سُكِّرَتْ أَبْصَٰرُنَا بَلْ نَحْنُ قَوْمٌ مَّسْحُورُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "16",
        "Recitation":  "Alafasy_128kbps/015016",
        "Body":  "وَلَقَدْ جَعَلْنَا فِى ٱلسَّمَآءِ بُرُوجًا وَزَيَّنَّٰهَا لِلنَّٰظِرِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "17",
        "Recitation":  "Alafasy_128kbps/015017",
        "Body":  "وَحَفِظْنَٰهَا مِن كُلِّ شَيْطَٰنٍ رَّجِيمٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "18",
        "Recitation":  "Alafasy_128kbps/015018",
        "Body":  "إِلَّا مَنِ ٱسْتَرَقَ ٱلسَّمْعَ فَأَتْبَعَهُۥ شِهَابٌ مُّبِينٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "19",
        "Recitation":  "Alafasy_128kbps/015019",
        "Body":  "وَٱلْأَرْضَ مَدَدْنَٰهَا وَأَلْقَيْنَا فِيهَا رَوَٰسِىَ وَأَنۢبَتْنَا فِيهَا مِن كُلِّ شَىْءٍ مَّوْزُونٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "20",
        "Recitation":  "Alafasy_128kbps/015020",
        "Body":  "وَجَعَلْنَا لَكُمْ فِيهَا مَعَٰيِشَ وَمَن لَّسْتُمْ لَهُۥ بِرَٰزِقِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "21",
        "Recitation":  "Alafasy_128kbps/015021",
        "Body":  "وَإِن مِّن شَىْءٍ إِلَّا عِندَنَا خَزَآئِنُهُۥ وَمَا نُنَزِّلُهُۥٓ إِلَّا بِقَدَرٍ مَّعْلُومٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "22",
        "Recitation":  "Alafasy_128kbps/015022",
        "Body":  "وَأَرْسَلْنَا ٱلرِّيَٰحَ لَوَٰقِحَ فَأَنزَلْنَا مِنَ ٱلسَّمَآءِ مَآءً فَأَسْقَيْنَٰكُمُوهُ وَمَآ أَنتُمْ لَهُۥ بِخَٰزِنِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "23",
        "Recitation":  "Alafasy_128kbps/015023",
        "Body":  "وَإِنَّا لَنَحْنُ نُحْىِۦ وَنُمِيتُ وَنَحْنُ ٱلْوَٰرِثُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "24",
        "Recitation":  "Alafasy_128kbps/015024",
        "Body":  "وَلَقَدْ عَلِمْنَا ٱلْمُسْتَقْدِمِينَ مِنكُمْ وَلَقَدْ عَلِمْنَا ٱلْمُسْتَـْٔخِرِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "25",
        "Recitation":  "Alafasy_128kbps/015025",
        "Body":  "وَإِنَّ رَبَّكَ هُوَ يَحْشُرُهُمْ ۚ إِنَّهُۥ حَكِيمٌ عَلِيمٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "26",
        "Recitation":  "Alafasy_128kbps/015026",
        "Body":  "وَلَقَدْ خَلَقْنَا ٱلْإِنسَٰنَ مِن صَلْصَٰلٍ مِّنْ حَمَإٍ مَّسْنُونٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "27",
        "Recitation":  "Alafasy_128kbps/015027",
        "Body":  "وَٱلْجَآنَّ خَلَقْنَٰهُ مِن قَبْلُ مِن نَّارِ ٱلسَّمُومِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "28",
        "Recitation":  "Alafasy_128kbps/015028",
        "Body":  "وَإِذْ قَالَ رَبُّكَ لِلْمَلَٰٓئِكَةِ إِنِّى خَٰلِقٌۢ بَشَرًا مِّن صَلْصَٰلٍ مِّنْ حَمَإٍ مَّسْنُونٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "29",
        "Recitation":  "Alafasy_128kbps/015029",
        "Body":  "فَإِذَا سَوَّيْتُهُۥ وَنَفَخْتُ فِيهِ مِن رُّوحِى فَقَعُوا۟ لَهُۥ سَٰجِدِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "30",
        "Recitation":  "Alafasy_128kbps/015030",
        "Body":  "فَسَجَدَ ٱلْمَلَٰٓئِكَةُ كُلُّهُمْ أَجْمَعُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "31",
        "Recitation":  "Alafasy_128kbps/015031",
        "Body":  "إِلَّآ إِبْلِيسَ أَبَىٰٓ أَن يَكُونَ مَعَ ٱلسَّٰجِدِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "32",
        "Recitation":  "Alafasy_128kbps/015032",
        "Body":  "قَالَ يَٰٓإِبْلِيسُ مَا لَكَ أَلَّا تَكُونَ مَعَ ٱلسَّٰجِدِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "33",
        "Recitation":  "Alafasy_128kbps/015033",
        "Body":  "قَالَ لَمْ أَكُن لِّأَسْجُدَ لِبَشَرٍ خَلَقْتَهُۥ مِن صَلْصَٰلٍ مِّنْ حَمَإٍ مَّسْنُونٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "34",
        "Recitation":  "Alafasy_128kbps/015034",
        "Body":  "قَالَ فَٱخْرُجْ مِنْهَا فَإِنَّكَ رَجِيمٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "35",
        "Recitation":  "Alafasy_128kbps/015035",
        "Body":  "وَإِنَّ عَلَيْكَ ٱللَّعْنَةَ إِلَىٰ يَوْمِ ٱلدِّينِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "36",
        "Recitation":  "Alafasy_128kbps/015036",
        "Body":  "قَالَ رَبِّ فَأَنظِرْنِىٓ إِلَىٰ يَوْمِ يُبْعَثُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "37",
        "Recitation":  "Alafasy_128kbps/015037",
        "Body":  "قَالَ فَإِنَّكَ مِنَ ٱلْمُنظَرِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "38",
        "Recitation":  "Alafasy_128kbps/015038",
        "Body":  "إِلَىٰ يَوْمِ ٱلْوَقْتِ ٱلْمَعْلُومِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "39",
        "Recitation":  "Alafasy_128kbps/015039",
        "Body":  "قَالَ رَبِّ بِمَآ أَغْوَيْتَنِى لَأُزَيِّنَنَّ لَهُمْ فِى ٱلْأَرْضِ وَلَأُغْوِيَنَّهُمْ أَجْمَعِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "40",
        "Recitation":  "Alafasy_128kbps/015040",
        "Body":  "إِلَّا عِبَادَكَ مِنْهُمُ ٱلْمُخْلَصِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "41",
        "Recitation":  "Alafasy_128kbps/015041",
        "Body":  "قَالَ هَٰذَا صِرَٰطٌ عَلَىَّ مُسْتَقِيمٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "42",
        "Recitation":  "Alafasy_128kbps/015042",
        "Body":  "إِنَّ عِبَادِى لَيْسَ لَكَ عَلَيْهِمْ سُلْطَٰنٌ إِلَّا مَنِ ٱتَّبَعَكَ مِنَ ٱلْغَاوِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "43",
        "Recitation":  "Alafasy_128kbps/015043",
        "Body":  "وَإِنَّ جَهَنَّمَ لَمَوْعِدُهُمْ أَجْمَعِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "44",
        "Recitation":  "Alafasy_128kbps/015044",
        "Body":  "لَهَا سَبْعَةُ أَبْوَٰبٍ لِّكُلِّ بَابٍ مِّنْهُمْ جُزْءٌ مَّقْسُومٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "45",
        "Recitation":  "Alafasy_128kbps/015045",
        "Body":  "إِنَّ ٱلْمُتَّقِينَ فِى جَنَّٰتٍ وَعُيُونٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "46",
        "Recitation":  "Alafasy_128kbps/015046",
        "Body":  "ٱدْخُلُوهَا بِسَلَٰمٍ ءَامِنِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "47",
        "Recitation":  "Alafasy_128kbps/015047",
        "Body":  "وَنَزَعْنَا مَا فِى صُدُورِهِم مِّنْ غِلٍّ إِخْوَٰنًا عَلَىٰ سُرُرٍ مُّتَقَٰبِلِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "48",
        "Recitation":  "Alafasy_128kbps/015048",
        "Body":  "لَا يَمَسُّهُمْ فِيهَا نَصَبٌ وَمَا هُم مِّنْهَا بِمُخْرَجِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "49",
        "Recitation":  "Alafasy_128kbps/015049",
        "Body":  "۞ نَبِّئْ عِبَادِىٓ أَنِّىٓ أَنَا ٱلْغَفُورُ ٱلرَّحِيمُ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "50",
        "Recitation":  "Alafasy_128kbps/015050",
        "Body":  "وَأَنَّ عَذَابِى هُوَ ٱلْعَذَابُ ٱلْأَلِيمُ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "51",
        "Recitation":  "Alafasy_128kbps/015051",
        "Body":  "وَنَبِّئْهُمْ عَن ضَيْفِ إِبْرَٰهِيمَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "52",
        "Recitation":  "Alafasy_128kbps/015052",
        "Body":  "إِذْ دَخَلُوا۟ عَلَيْهِ فَقَالُوا۟ سَلَٰمًا قَالَ إِنَّا مِنكُمْ وَجِلُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "53",
        "Recitation":  "Alafasy_128kbps/015053",
        "Body":  "قَالُوا۟ لَا تَوْجَلْ إِنَّا نُبَشِّرُكَ بِغُلَٰمٍ عَلِيمٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "54",
        "Recitation":  "Alafasy_128kbps/015054",
        "Body":  "قَالَ أَبَشَّرْتُمُونِى عَلَىٰٓ أَن مَّسَّنِىَ ٱلْكِبَرُ فَبِمَ تُبَشِّرُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "55",
        "Recitation":  "Alafasy_128kbps/015055",
        "Body":  "قَالُوا۟ بَشَّرْنَٰكَ بِٱلْحَقِّ فَلَا تَكُن مِّنَ ٱلْقَٰنِطِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "56",
        "Recitation":  "Alafasy_128kbps/015056",
        "Body":  "قَالَ وَمَن يَقْنَطُ مِن رَّحْمَةِ رَبِّهِۦٓ إِلَّا ٱلضَّآلُّونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "57",
        "Recitation":  "Alafasy_128kbps/015057",
        "Body":  "قَالَ فَمَا خَطْبُكُمْ أَيُّهَا ٱلْمُرْسَلُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "58",
        "Recitation":  "Alafasy_128kbps/015058",
        "Body":  "قَالُوٓا۟ إِنَّآ أُرْسِلْنَآ إِلَىٰ قَوْمٍ مُّجْرِمِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "59",
        "Recitation":  "Alafasy_128kbps/015059",
        "Body":  "إِلَّآ ءَالَ لُوطٍ إِنَّا لَمُنَجُّوهُمْ أَجْمَعِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "60",
        "Recitation":  "Alafasy_128kbps/015060",
        "Body":  "إِلَّا ٱمْرَأَتَهُۥ قَدَّرْنَآ ۙ إِنَّهَا لَمِنَ ٱلْغَٰبِرِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "61",
        "Recitation":  "Alafasy_128kbps/015061",
        "Body":  "فَلَمَّا جَآءَ ءَالَ لُوطٍ ٱلْمُرْسَلُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "62",
        "Recitation":  "Alafasy_128kbps/015062",
        "Body":  "قَالَ إِنَّكُمْ قَوْمٌ مُّنكَرُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "63",
        "Recitation":  "Alafasy_128kbps/015063",
        "Body":  "قَالُوا۟ بَلْ جِئْنَٰكَ بِمَا كَانُوا۟ فِيهِ يَمْتَرُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "64",
        "Recitation":  "Alafasy_128kbps/015064",
        "Body":  "وَأَتَيْنَٰكَ بِٱلْحَقِّ وَإِنَّا لَصَٰدِقُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "65",
        "Recitation":  "Alafasy_128kbps/015065",
        "Body":  "فَأَسْرِ بِأَهْلِكَ بِقِطْعٍ مِّنَ ٱلَّيْلِ وَٱتَّبِعْ أَدْبَٰرَهُمْ وَلَا يَلْتَفِتْ مِنكُمْ أَحَدٌ وَٱمْضُوا۟ حَيْثُ تُؤْمَرُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "66",
        "Recitation":  "Alafasy_128kbps/015066",
        "Body":  "وَقَضَيْنَآ إِلَيْهِ ذَٰلِكَ ٱلْأَمْرَ أَنَّ دَابِرَ هَٰٓؤُلَآءِ مَقْطُوعٌ مُّصْبِحِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "67",
        "Recitation":  "Alafasy_128kbps/015067",
        "Body":  "وَجَآءَ أَهْلُ ٱلْمَدِينَةِ يَسْتَبْشِرُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "68",
        "Recitation":  "Alafasy_128kbps/015068",
        "Body":  "قَالَ إِنَّ هَٰٓؤُلَآءِ ضَيْفِى فَلَا تَفْضَحُونِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "69",
        "Recitation":  "Alafasy_128kbps/015069",
        "Body":  "وَٱتَّقُوا۟ ٱللَّهَ وَلَا تُخْزُونِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "70",
        "Recitation":  "Alafasy_128kbps/015070",
        "Body":  "قَالُوٓا۟ أَوَلَمْ نَنْهَكَ عَنِ ٱلْعَٰلَمِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "71",
        "Recitation":  "Alafasy_128kbps/015071",
        "Body":  "قَالَ هَٰٓؤُلَآءِ بَنَاتِىٓ إِن كُنتُمْ فَٰعِلِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "72",
        "Recitation":  "Alafasy_128kbps/015072",
        "Body":  "لَعَمْرُكَ إِنَّهُمْ لَفِى سَكْرَتِهِمْ يَعْمَهُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "73",
        "Recitation":  "Alafasy_128kbps/015073",
        "Body":  "فَأَخَذَتْهُمُ ٱلصَّيْحَةُ مُشْرِقِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "74",
        "Recitation":  "Alafasy_128kbps/015074",
        "Body":  "فَجَعَلْنَا عَٰلِيَهَا سَافِلَهَا وَأَمْطَرْنَا عَلَيْهِمْ حِجَارَةً مِّن سِجِّيلٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "75",
        "Recitation":  "Alafasy_128kbps/015075",
        "Body":  "إِنَّ فِى ذَٰلِكَ لَءَايَٰتٍ لِّلْمُتَوَسِّمِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "76",
        "Recitation":  "Alafasy_128kbps/015076",
        "Body":  "وَإِنَّهَا لَبِسَبِيلٍ مُّقِيمٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "77",
        "Recitation":  "Alafasy_128kbps/015077",
        "Body":  "إِنَّ فِى ذَٰلِكَ لَءَايَةً لِّلْمُؤْمِنِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "78",
        "Recitation":  "Alafasy_128kbps/015078",
        "Body":  "وَإِن كَانَ أَصْحَٰبُ ٱلْأَيْكَةِ لَظَٰلِمِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "79",
        "Recitation":  "Alafasy_128kbps/015079",
        "Body":  "فَٱنتَقَمْنَا مِنْهُمْ وَإِنَّهُمَا لَبِإِمَامٍ مُّبِينٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "80",
        "Recitation":  "Alafasy_128kbps/015080",
        "Body":  "وَلَقَدْ كَذَّبَ أَصْحَٰبُ ٱلْحِجْرِ ٱلْمُرْسَلِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "81",
        "Recitation":  "Alafasy_128kbps/015081",
        "Body":  "وَءَاتَيْنَٰهُمْ ءَايَٰتِنَا فَكَانُوا۟ عَنْهَا مُعْرِضِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "82",
        "Recitation":  "Alafasy_128kbps/015082",
        "Body":  "وَكَانُوا۟ يَنْحِتُونَ مِنَ ٱلْجِبَالِ بُيُوتًا ءَامِنِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "83",
        "Recitation":  "Alafasy_128kbps/015083",
        "Body":  "فَأَخَذَتْهُمُ ٱلصَّيْحَةُ مُصْبِحِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "84",
        "Recitation":  "Alafasy_128kbps/015084",
        "Body":  "فَمَآ أَغْنَىٰ عَنْهُم مَّا كَانُوا۟ يَكْسِبُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "85",
        "Recitation":  "Alafasy_128kbps/015085",
        "Body":  "وَمَا خَلَقْنَا ٱلسَّمَٰوَٰتِ وَٱلْأَرْضَ وَمَا بَيْنَهُمَآ إِلَّا بِٱلْحَقِّ ۗ وَإِنَّ ٱلسَّاعَةَ لَءَاتِيَةٌ ۖ فَٱصْفَحِ ٱلصَّفْحَ ٱلْجَمِيلَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "86",
        "Recitation":  "Alafasy_128kbps/015086",
        "Body":  "إِنَّ رَبَّكَ هُوَ ٱلْخَلَّٰقُ ٱلْعَلِيمُ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "87",
        "Recitation":  "Alafasy_128kbps/015087",
        "Body":  "وَلَقَدْ ءَاتَيْنَٰكَ سَبْعًا مِّنَ ٱلْمَثَانِى وَٱلْقُرْءَانَ ٱلْعَظِيمَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "88",
        "Recitation":  "Alafasy_128kbps/015088",
        "Body":  "لَا تَمُدَّنَّ عَيْنَيْكَ إِلَىٰ مَا مَتَّعْنَا بِهِۦٓ أَزْوَٰجًا مِّنْهُمْ وَلَا تَحْزَنْ عَلَيْهِمْ وَٱخْفِضْ جَنَاحَكَ لِلْمُؤْمِنِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "89",
        "Recitation":  "Alafasy_128kbps/015089",
        "Body":  "وَقُلْ إِنِّىٓ أَنَا ٱلنَّذِيرُ ٱلْمُبِينُ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "90",
        "Recitation":  "Alafasy_128kbps/015090",
        "Body":  "كَمَآ أَنزَلْنَا عَلَى ٱلْمُقْتَسِمِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "91",
        "Recitation":  "Alafasy_128kbps/015091",
        "Body":  "ٱلَّذِينَ جَعَلُوا۟ ٱلْقُرْءَانَ عِضِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "92",
        "Recitation":  "Alafasy_128kbps/015092",
        "Body":  "فَوَرَبِّكَ لَنَسْـَٔلَنَّهُمْ أَجْمَعِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "93",
        "Recitation":  "Alafasy_128kbps/015093",
        "Body":  "عَمَّا كَانُوا۟ يَعْمَلُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "94",
        "Recitation":  "Alafasy_128kbps/015094",
        "Body":  "فَٱصْدَعْ بِمَا تُؤْمَرُ وَأَعْرِضْ عَنِ ٱلْمُشْرِكِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "95",
        "Recitation":  "Alafasy_128kbps/015095",
        "Body":  "إِنَّا كَفَيْنَٰكَ ٱلْمُسْتَهْزِءِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "96",
        "Recitation":  "Alafasy_128kbps/015096",
        "Body":  "ٱلَّذِينَ يَجْعَلُونَ مَعَ ٱللَّهِ إِلَٰهًا ءَاخَرَ ۚ فَسَوْفَ يَعْلَمُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "97",
        "Recitation":  "Alafasy_128kbps/015097",
        "Body":  "وَلَقَدْ نَعْلَمُ أَنَّكَ يَضِيقُ صَدْرُكَ بِمَا يَقُولُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "98",
        "Recitation":  "Alafasy_128kbps/015098",
        "Body":  "فَسَبِّحْ بِحَمْدِ رَبِّكَ وَكُن مِّنَ ٱلسَّٰجِدِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "15",
        "AyahNumber":  "99",
        "Recitation":  "Alafasy_128kbps/015099",
        "Body":  "وَٱعْبُدْ رَبَّكَ حَتَّىٰ يَأْتِيَكَ ٱلْيَقِينُ"
    }
]
