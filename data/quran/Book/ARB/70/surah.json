        "SurahNumber":  "70",
        "AyahNumber":  "2",
        "Recitation":  "Alafasy_128kbps/070002",
        "Body":  "لِّلْكَٰفِرِينَ لَيْسَ لَهُۥ دَافِعٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "3",
        "Recitation":  "Alafasy_128kbps/070003",
        "Body":  "مِّنَ ٱللَّهِ ذِى ٱلْمَعَارِجِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "4",
        "Recitation":  "Alafasy_128kbps/070004",
        "Body":  "تَعْرُجُ ٱلْمَلَٰٓئِكَةُ وَٱلرُّوحُ إِلَيْهِ فِى يَوْمٍ كَانَ مِقْدَارُهُۥ خَمْسِينَ أَلْفَ سَنَةٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "5",
        "Recitation":  "Alafasy_128kbps/070005",
        "Body":  "فَٱصْبِرْ صَبْرًا جَمِيلًا"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "6",
        "Recitation":  "Alafasy_128kbps/070006",
        "Body":  "إِنَّهُمْ يَرَوْنَهُۥ بَعِيدًا"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "7",
        "Recitation":  "Alafasy_128kbps/070007",
        "Body":  "وَنَرَىٰهُ قَرِيبًا"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "8",
        "Recitation":  "Alafasy_128kbps/070008",
        "Body":  "يَوْمَ تَكُونُ ٱلسَّمَآءُ كَٱلْمُهْلِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "9",
        "Recitation":  "Alafasy_128kbps/070009",
        "Body":  "وَتَكُونُ ٱلْجِبَالُ كَٱلْعِهْنِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "10",
        "Recitation":  "Alafasy_128kbps/070010",
        "Body":  "وَلَا يَسْـَٔلُ حَمِيمٌ حَمِيمًا"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "11",
        "Recitation":  "Alafasy_128kbps/070011",
        "Body":  "يُبَصَّرُونَهُمْ ۚ يَوَدُّ ٱلْمُجْرِمُ لَوْ يَفْتَدِى مِنْ عَذَابِ يَوْمِئِذٍۭ بِبَنِيهِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "12",
        "Recitation":  "Alafasy_128kbps/070012",
        "Body":  "وَصَٰحِبَتِهِۦ وَأَخِيهِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "13",
        "Recitation":  "Alafasy_128kbps/070013",
        "Body":  "وَفَصِيلَتِهِ ٱلَّتِى تُـْٔوِيهِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "14",
        "Recitation":  "Alafasy_128kbps/070014",
        "Body":  "وَمَن فِى ٱلْأَرْضِ جَمِيعًا ثُمَّ يُنجِيهِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "15",
        "Recitation":  "Alafasy_128kbps/070015",
        "Body":  "كَلَّآ ۖ إِنَّهَا لَظَىٰ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "16",
        "Recitation":  "Alafasy_128kbps/070016",
        "Body":  "نَزَّاعَةً لِّلشَّوَىٰ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "17",
        "Recitation":  "Alafasy_128kbps/070017",
        "Body":  "تَدْعُوا۟ مَنْ أَدْبَرَ وَتَوَلَّىٰ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "18",
        "Recitation":  "Alafasy_128kbps/070018",
        "Body":  "وَجَمَعَ فَأَوْعَىٰٓ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "19",
        "Recitation":  "Alafasy_128kbps/070019",
        "Body":  "۞ إِنَّ ٱلْإِنسَٰنَ خُلِقَ هَلُوعًا"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "20",
        "Recitation":  "Alafasy_128kbps/070020",
        "Body":  "إِذَا مَسَّهُ ٱلشَّرُّ جَزُوعًا"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "21",
        "Recitation":  "Alafasy_128kbps/070021",
        "Body":  "وَإِذَا مَسَّهُ ٱلْخَيْرُ مَنُوعًا"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "22",
        "Recitation":  "Alafasy_128kbps/070022",
        "Body":  "إِلَّا ٱلْمُصَلِّينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "23",
        "Recitation":  "Alafasy_128kbps/070023",
        "Body":  "ٱلَّذِينَ هُمْ عَلَىٰ صَلَاتِهِمْ دَآئِمُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "24",
        "Recitation":  "Alafasy_128kbps/070024",
        "Body":  "وَٱلَّذِينَ فِىٓ أَمْوَٰلِهِمْ حَقٌّ مَّعْلُومٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "25",
        "Recitation":  "Alafasy_128kbps/070025",
        "Body":  "لِّلسَّآئِلِ وَٱلْمَحْرُومِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "26",
        "Recitation":  "Alafasy_128kbps/070026",
        "Body":  "وَٱلَّذِينَ يُصَدِّقُونَ بِيَوْمِ ٱلدِّينِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "27",
        "Recitation":  "Alafasy_128kbps/070027",
        "Body":  "وَٱلَّذِينَ هُم مِّنْ عَذَابِ رَبِّهِم مُّشْفِقُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "28",
        "Recitation":  "Alafasy_128kbps/070028",
        "Body":  "إِنَّ عَذَابَ رَبِّهِمْ غَيْرُ مَأْمُونٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "29",
        "Recitation":  "Alafasy_128kbps/070029",
        "Body":  "وَٱلَّذِينَ هُمْ لِفُرُوجِهِمْ حَٰفِظُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "30",
        "Recitation":  "Alafasy_128kbps/070030",
        "Body":  "إِلَّا عَلَىٰٓ أَزْوَٰجِهِمْ أَوْ مَا مَلَكَتْ أَيْمَٰنُهُمْ فَإِنَّهُمْ غَيْرُ مَلُومِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "31",
        "Recitation":  "Alafasy_128kbps/070031",
        "Body":  "فَمَنِ ٱبْتَغَىٰ وَرَآءَ ذَٰلِكَ فَأُو۟لَٰٓئِكَ هُمُ ٱلْعَادُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "32",
        "Recitation":  "Alafasy_128kbps/070032",
        "Body":  "وَٱلَّذِينَ هُمْ لِأَمَٰنَٰتِهِمْ وَعَهْدِهِمْ رَٰعُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "33",
        "Recitation":  "Alafasy_128kbps/070033",
        "Body":  "وَٱلَّذِينَ هُم بِشَهَٰدَٰتِهِمْ قَآئِمُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "34",
        "Recitation":  "Alafasy_128kbps/070034",
        "Body":  "وَٱلَّذِينَ هُمْ عَلَىٰ صَلَاتِهِمْ يُحَافِظُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "35",
        "Recitation":  "Alafasy_128kbps/070035",
        "Body":  "أُو۟لَٰٓئِكَ فِى جَنَّٰتٍ مُّكْرَمُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "36",
        "Recitation":  "Alafasy_128kbps/070036",
        "Body":  "فَمَالِ ٱلَّذِينَ كَفَرُوا۟ قِبَلَكَ مُهْطِعِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "37",
        "Recitation":  "Alafasy_128kbps/070037",
        "Body":  "عَنِ ٱلْيَمِينِ وَعَنِ ٱلشِّمَالِ عِزِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "38",
        "Recitation":  "Alafasy_128kbps/070038",
        "Body":  "أَيَطْمَعُ كُلُّ ٱمْرِئٍ مِّنْهُمْ أَن يُدْخَلَ جَنَّةَ نَعِيمٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "39",
        "Recitation":  "Alafasy_128kbps/070039",
        "Body":  "كَلَّآ ۖ إِنَّا خَلَقْنَٰهُم مِّمَّا يَعْلَمُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "40",
        "Recitation":  "Alafasy_128kbps/070040",
        "Body":  "فَلَآ أُقْسِمُ بِرَبِّ ٱلْمَشَٰرِقِ وَٱلْمَغَٰرِبِ إِنَّا لَقَٰدِرُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "41",
        "Recitation":  "Alafasy_128kbps/070041",
        "Body":  "عَلَىٰٓ أَن نُّبَدِّلَ خَيْرًا مِّنْهُمْ وَمَا نَحْنُ بِمَسْبُوقِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "42",
        "Recitation":  "Alafasy_128kbps/070042",
        "Body":  "فَذَرْهُمْ يَخُوضُوا۟ وَيَلْعَبُوا۟ حَتَّىٰ يُلَٰقُوا۟ يَوْمَهُمُ ٱلَّذِى يُوعَدُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "43",
        "Recitation":  "Alafasy_128kbps/070043",
        "Body":  "يَوْمَ يَخْرُجُونَ مِنَ ٱلْأَجْدَاثِ سِرَاعًا كَأَنَّهُمْ إِلَىٰ نُصُبٍ يُوفِضُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "70",
        "AyahNumber":  "44",
        "Recitation":  "Alafasy_128kbps/070044",
        "Body":  "خَٰشِعَةً أَبْصَٰرُهُمْ تَرْهَقُهُمْ ذِلَّةٌ ۚ ذَٰلِكَ ٱلْيَوْمُ ٱلَّذِى كَانُوا۟ يُوعَدُونَ"
    }
]
