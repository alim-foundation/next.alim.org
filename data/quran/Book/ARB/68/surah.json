[
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "1",
        "Recitation":  "Alafasy_128kbps/068001",
        "Body":  "نٓ ۚ وَٱلْقَلَمِ وَمَا يَسْطُرُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "2",
        "Recitation":  "Alafasy_128kbps/068002",
        "Body":  "مَآ أَنتَ بِنِعْمَةِ رَبِّكَ بِمَجْنُونٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "3",
        "Recitation":  "Alafasy_128kbps/068003",
        "Body":  "وَإِنَّ لَكَ لَأَجْرًا غَيْرَ مَمْنُونٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "4",
        "Recitation":  "Alafasy_128kbps/068004",
        "Body":  "وَإِنَّكَ لَعَلَىٰ خُلُقٍ عَظِيمٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "5",
        "Recitation":  "Alafasy_128kbps/068005",
        "Body":  "فَسَتُبْصِرُ وَيُبْصِرُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "6",
        "Recitation":  "Alafasy_128kbps/068006",
        "Body":  "بِأَييِّكُمُ ٱلْمَفْتُونُ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "7",
        "Recitation":  "Alafasy_128kbps/068007",
        "Body":  "إِنَّ رَبَّكَ هُوَ أَعْلَمُ بِمَن ضَلَّ عَن سَبِيلِهِۦ وَهُوَ أَعْلَمُ بِٱلْمُهْتَدِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "8",
        "Recitation":  "Alafasy_128kbps/068008",
        "Body":  "فَلَا تُطِعِ ٱلْمُكَذِّبِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "9",
        "Recitation":  "Alafasy_128kbps/068009",
        "Body":  "وَدُّوا۟ لَوْ تُدْهِنُ فَيُدْهِنُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "10",
        "Recitation":  "Alafasy_128kbps/068010",
        "Body":  "وَلَا تُطِعْ كُلَّ حَلَّافٍ مَّهِينٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "11",
        "Recitation":  "Alafasy_128kbps/068011",
        "Body":  "هَمَّازٍ مَّشَّآءٍۭ بِنَمِيمٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "12",
        "Recitation":  "Alafasy_128kbps/068012",
        "Body":  "مَّنَّاعٍ لِّلْخَيْرِ مُعْتَدٍ أَثِيمٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "13",
        "Recitation":  "Alafasy_128kbps/068013",
        "Body":  "عُتُلٍّۭ بَعْدَ ذَٰلِكَ زَنِيمٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "14",
        "Recitation":  "Alafasy_128kbps/068014",
        "Body":  "أَن كَانَ ذَا مَالٍ وَبَنِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "15",
        "Recitation":  "Alafasy_128kbps/068015",
        "Body":  "إِذَا تُتْلَىٰ عَلَيْهِ ءَايَٰتُنَا قَالَ أَسَٰطِيرُ ٱلْأَوَّلِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "16",
        "Recitation":  "Alafasy_128kbps/068016",
        "Body":  "سَنَسِمُهُۥ عَلَى ٱلْخُرْطُومِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "17",
        "Recitation":  "Alafasy_128kbps/068017",
        "Body":  "إِنَّا بَلَوْنَٰهُمْ كَمَا بَلَوْنَآ أَصْحَٰبَ ٱلْجَنَّةِ إِذْ أَقْسَمُوا۟ لَيَصْرِمُنَّهَا مُصْبِحِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "18",
        "Recitation":  "Alafasy_128kbps/068018",
        "Body":  "وَلَا يَسْتَثْنُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "19",
        "Recitation":  "Alafasy_128kbps/068019",
        "Body":  "فَطَافَ عَلَيْهَا طَآئِفٌ مِّن رَّبِّكَ وَهُمْ نَآئِمُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "20",
        "Recitation":  "Alafasy_128kbps/068020",
        "Body":  "فَأَصْبَحَتْ كَٱلصَّرِيمِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "21",
        "Recitation":  "Alafasy_128kbps/068021",
        "Body":  "فَتَنَادَوْا۟ مُصْبِحِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "22",
        "Recitation":  "Alafasy_128kbps/068022",
        "Body":  "أَنِ ٱغْدُوا۟ عَلَىٰ حَرْثِكُمْ إِن كُنتُمْ صَٰرِمِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "23",
        "Recitation":  "Alafasy_128kbps/068023",
        "Body":  "فَٱنطَلَقُوا۟ وَهُمْ يَتَخَٰفَتُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "24",
        "Recitation":  "Alafasy_128kbps/068024",
        "Body":  "أَن لَّا يَدْخُلَنَّهَا ٱلْيَوْمَ عَلَيْكُم مِّسْكِينٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "25",
        "Recitation":  "Alafasy_128kbps/068025",
        "Body":  "وَغَدَوْا۟ عَلَىٰ حَرْدٍ قَٰدِرِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "26",
        "Recitation":  "Alafasy_128kbps/068026",
        "Body":  "فَلَمَّا رَأَوْهَا قَالُوٓا۟ إِنَّا لَضَآلُّونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "27",
        "Recitation":  "Alafasy_128kbps/068027",
        "Body":  "بَلْ نَحْنُ مَحْرُومُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "28",
        "Recitation":  "Alafasy_128kbps/068028",
        "Body":  "قَالَ أَوْسَطُهُمْ أَلَمْ أَقُل لَّكُمْ لَوْلَا تُسَبِّحُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "29",
        "Recitation":  "Alafasy_128kbps/068029",
        "Body":  "قَالُوا۟ سُبْحَٰنَ رَبِّنَآ إِنَّا كُنَّا ظَٰلِمِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "30",
        "Recitation":  "Alafasy_128kbps/068030",
        "Body":  "فَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَلَٰوَمُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "31",
        "Recitation":  "Alafasy_128kbps/068031",
        "Body":  "قَالُوا۟ يَٰوَيْلَنَآ إِنَّا كُنَّا طَٰغِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "32",
        "Recitation":  "Alafasy_128kbps/068032",
        "Body":  "عَسَىٰ رَبُّنَآ أَن يُبْدِلَنَا خَيْرًا مِّنْهَآ إِنَّآ إِلَىٰ رَبِّنَا رَٰغِبُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "33",
        "Recitation":  "Alafasy_128kbps/068033",
        "Body":  "كَذَٰلِكَ ٱلْعَذَابُ ۖ وَلَعَذَابُ ٱلْءَاخِرَةِ أَكْبَرُ ۚ لَوْ كَانُوا۟ يَعْلَمُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "34",
        "Recitation":  "Alafasy_128kbps/068034",
        "Body":  "إِنَّ لِلْمُتَّقِينَ عِندَ رَبِّهِمْ جَنَّٰتِ ٱلنَّعِيمِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "35",
        "Recitation":  "Alafasy_128kbps/068035",
        "Body":  "أَفَنَجْعَلُ ٱلْمُسْلِمِينَ كَٱلْمُجْرِمِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "36",
        "Recitation":  "Alafasy_128kbps/068036",
        "Body":  "مَا لَكُمْ كَيْفَ تَحْكُمُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "37",
        "Recitation":  "Alafasy_128kbps/068037",
        "Body":  "أَمْ لَكُمْ كِتَٰبٌ فِيهِ تَدْرُسُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "38",
        "Recitation":  "Alafasy_128kbps/068038",
        "Body":  "إِنَّ لَكُمْ فِيهِ لَمَا تَخَيَّرُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "39",
        "Recitation":  "Alafasy_128kbps/068039",
        "Body":  "أَمْ لَكُمْ أَيْمَٰنٌ عَلَيْنَا بَٰلِغَةٌ إِلَىٰ يَوْمِ ٱلْقِيَٰمَةِ ۙ إِنَّ لَكُمْ لَمَا تَحْكُمُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "40",
        "Recitation":  "Alafasy_128kbps/068040",
        "Body":  "سَلْهُمْ أَيُّهُم بِذَٰلِكَ زَعِيمٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "41",
        "Recitation":  "Alafasy_128kbps/068041",
        "Body":  "أَمْ لَهُمْ شُرَكَآءُ فَلْيَأْتُوا۟ بِشُرَكَآئِهِمْ إِن كَانُوا۟ صَٰدِقِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "42",
        "Recitation":  "Alafasy_128kbps/068042",
        "Body":  "يَوْمَ يُكْشَفُ عَن سَاقٍ وَيُدْعَوْنَ إِلَى ٱلسُّجُودِ فَلَا يَسْتَطِيعُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "43",
        "Recitation":  "Alafasy_128kbps/068043",
        "Body":  "خَٰشِعَةً أَبْصَٰرُهُمْ تَرْهَقُهُمْ ذِلَّةٌ ۖ وَقَدْ كَانُوا۟ يُدْعَوْنَ إِلَى ٱلسُّجُودِ وَهُمْ سَٰلِمُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "44",
        "Recitation":  "Alafasy_128kbps/068044",
        "Body":  "فَذَرْنِى وَمَن يُكَذِّبُ بِهَٰذَا ٱلْحَدِيثِ ۖ سَنَسْتَدْرِجُهُم مِّنْ حَيْثُ لَا يَعْلَمُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "45",
        "Recitation":  "Alafasy_128kbps/068045",
        "Body":  "وَأُمْلِى لَهُمْ ۚ إِنَّ كَيْدِى مَتِينٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "46",
        "Recitation":  "Alafasy_128kbps/068046",
        "Body":  "أَمْ تَسْـَٔلُهُمْ أَجْرًا فَهُم مِّن مَّغْرَمٍ مُّثْقَلُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "47",
        "Recitation":  "Alafasy_128kbps/068047",
        "Body":  "أَمْ عِندَهُمُ ٱلْغَيْبُ فَهُمْ يَكْتُبُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "48",
        "Recitation":  "Alafasy_128kbps/068048",
        "Body":  "فَٱصْبِرْ لِحُكْمِ رَبِّكَ وَلَا تَكُن كَصَاحِبِ ٱلْحُوتِ إِذْ نَادَىٰ وَهُوَ مَكْظُومٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "49",
        "Recitation":  "Alafasy_128kbps/068049",
        "Body":  "لَّوْلَآ أَن تَدَٰرَكَهُۥ نِعْمَةٌ مِّن رَّبِّهِۦ لَنُبِذَ بِٱلْعَرَآءِ وَهُوَ مَذْمُومٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "50",
        "Recitation":  "Alafasy_128kbps/068050",
        "Body":  "فَٱجْتَبَٰهُ رَبُّهُۥ فَجَعَلَهُۥ مِنَ ٱلصَّٰلِحِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "51",
        "Recitation":  "Alafasy_128kbps/068051",
        "Body":  "وَإِن يَكَادُ ٱلَّذِينَ كَفَرُوا۟ لَيُزْلِقُونَكَ بِأَبْصَٰرِهِمْ لَمَّا سَمِعُوا۟ ٱلذِّكْرَ وَيَقُولُونَ إِنَّهُۥ لَمَجْنُونٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "68",
        "AyahNumber":  "52",
        "Recitation":  "Alafasy_128kbps/068052",
        "Body":  "وَمَا هُوَ إِلَّا ذِكْرٌ لِّلْعَٰلَمِينَ"
    }
]
