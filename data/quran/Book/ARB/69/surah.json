[
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "1",
        "Recitation":  "Alafasy_128kbps/069001",
        "Body":  "ٱلْحَآقَّةُ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "2",
        "Recitation":  "Alafasy_128kbps/069002",
        "Body":  "مَا ٱلْحَآقَّةُ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "3",
        "Recitation":  "Alafasy_128kbps/069003",
        "Body":  "وَمَآ أَدْرَىٰكَ مَا ٱلْحَآقَّةُ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "4",
        "Recitation":  "Alafasy_128kbps/069004",
        "Body":  "كَذَّبَتْ ثَمُودُ وَعَادٌۢ بِٱلْقَارِعَةِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "5",
        "Recitation":  "Alafasy_128kbps/069005",
        "Body":  "فَأَمَّا ثَمُودُ فَأُهْلِكُوا۟ بِٱلطَّاغِيَةِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "6",
        "Recitation":  "Alafasy_128kbps/069006",
        "Body":  "وَأَمَّا عَادٌ فَأُهْلِكُوا۟ بِرِيحٍ صَرْصَرٍ عَاتِيَةٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "7",
        "Recitation":  "Alafasy_128kbps/069007",
        "Body":  "سَخَّرَهَا عَلَيْهِمْ سَبْعَ لَيَالٍ وَثَمَٰنِيَةَ أَيَّامٍ حُسُومًا فَتَرَى ٱلْقَوْمَ فِيهَا صَرْعَىٰ كَأَنَّهُمْ أَعْجَازُ نَخْلٍ خَاوِيَةٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "8",
        "Recitation":  "Alafasy_128kbps/069008",
        "Body":  "فَهَلْ تَرَىٰ لَهُم مِّنۢ بَاقِيَةٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "9",
        "Recitation":  "Alafasy_128kbps/069009",
        "Body":  "وَجَآءَ فِرْعَوْنُ وَمَن قَبْلَهُۥ وَٱلْمُؤْتَفِكَٰتُ بِٱلْخَاطِئَةِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "10",
        "Recitation":  "Alafasy_128kbps/069010",
        "Body":  "فَعَصَوْا۟ رَسُولَ رَبِّهِمْ فَأَخَذَهُمْ أَخْذَةً رَّابِيَةً"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "11",
        "Recitation":  "Alafasy_128kbps/069011",
        "Body":  "إِنَّا لَمَّا طَغَا ٱلْمَآءُ حَمَلْنَٰكُمْ فِى ٱلْجَارِيَةِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "12",
        "Recitation":  "Alafasy_128kbps/069012",
        "Body":  "لِنَجْعَلَهَا لَكُمْ تَذْكِرَةً وَتَعِيَهَآ أُذُنٌ وَٰعِيَةٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "13",
        "Recitation":  "Alafasy_128kbps/069013",
        "Body":  "فَإِذَا نُفِخَ فِى ٱلصُّورِ نَفْخَةٌ وَٰحِدَةٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "14",
        "Recitation":  "Alafasy_128kbps/069014",
        "Body":  "وَحُمِلَتِ ٱلْأَرْضُ وَٱلْجِبَالُ فَدُكَّتَا دَكَّةً وَٰحِدَةً"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "15",
        "Recitation":  "Alafasy_128kbps/069015",
        "Body":  "فَيَوْمَئِذٍ وَقَعَتِ ٱلْوَاقِعَةُ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "16",
        "Recitation":  "Alafasy_128kbps/069016",
        "Body":  "وَٱنشَقَّتِ ٱلسَّمَآءُ فَهِىَ يَوْمَئِذٍ وَاهِيَةٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "17",
        "Recitation":  "Alafasy_128kbps/069017",
        "Body":  "وَٱلْمَلَكُ عَلَىٰٓ أَرْجَآئِهَا ۚ وَيَحْمِلُ عَرْشَ رَبِّكَ فَوْقَهُمْ يَوْمَئِذٍ ثَمَٰنِيَةٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "18",
        "Recitation":  "Alafasy_128kbps/069018",
        "Body":  "يَوْمَئِذٍ تُعْرَضُونَ لَا تَخْفَىٰ مِنكُمْ خَافِيَةٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "19",
        "Recitation":  "Alafasy_128kbps/069019",
        "Body":  "فَأَمَّا مَنْ أُوتِىَ كِتَٰبَهُۥ بِيَمِينِهِۦ فَيَقُولُ هَآؤُمُ ٱقْرَءُوا۟ كِتَٰبِيَهْ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "20",
        "Recitation":  "Alafasy_128kbps/069020",
        "Body":  "إِنِّى ظَنَنتُ أَنِّى مُلَٰقٍ حِسَابِيَهْ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "21",
        "Recitation":  "Alafasy_128kbps/069021",
        "Body":  "فَهُوَ فِى عِيشَةٍ رَّاضِيَةٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "22",
        "Recitation":  "Alafasy_128kbps/069022",
        "Body":  "فِى جَنَّةٍ عَالِيَةٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "23",
        "Recitation":  "Alafasy_128kbps/069023",
        "Body":  "قُطُوفُهَا دَانِيَةٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "24",
        "Recitation":  "Alafasy_128kbps/069024",
        "Body":  "كُلُوا۟ وَٱشْرَبُوا۟ هَنِيٓـًٔۢا بِمَآ أَسْلَفْتُمْ فِى ٱلْأَيَّامِ ٱلْخَالِيَةِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "25",
        "Recitation":  "Alafasy_128kbps/069025",
        "Body":  "وَأَمَّا مَنْ أُوتِىَ كِتَٰبَهُۥ بِشِمَالِهِۦ فَيَقُولُ يَٰلَيْتَنِى لَمْ أُوتَ كِتَٰبِيَهْ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "26",
        "Recitation":  "Alafasy_128kbps/069026",
        "Body":  "وَلَمْ أَدْرِ مَا حِسَابِيَهْ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "27",
        "Recitation":  "Alafasy_128kbps/069027",
        "Body":  "يَٰلَيْتَهَا كَانَتِ ٱلْقَاضِيَةَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "28",
        "Recitation":  "Alafasy_128kbps/069028",
        "Body":  "مَآ أَغْنَىٰ عَنِّى مَالِيَهْ ۜ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "29",
        "Recitation":  "Alafasy_128kbps/069029",
        "Body":  "هَلَكَ عَنِّى سُلْطَٰنِيَهْ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "30",
        "Recitation":  "Alafasy_128kbps/069030",
        "Body":  "خُذُوهُ فَغُلُّوهُ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "31",
        "Recitation":  "Alafasy_128kbps/069031",
        "Body":  "ثُمَّ ٱلْجَحِيمَ صَلُّوهُ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "32",
        "Recitation":  "Alafasy_128kbps/069032",
        "Body":  "ثُمَّ فِى سِلْسِلَةٍ ذَرْعُهَا سَبْعُونَ ذِرَاعًا فَٱسْلُكُوهُ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "33",
        "Recitation":  "Alafasy_128kbps/069033",
        "Body":  "إِنَّهُۥ كَانَ لَا يُؤْمِنُ بِٱللَّهِ ٱلْعَظِيمِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "34",
        "Recitation":  "Alafasy_128kbps/069034",
        "Body":  "وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلْمِسْكِينِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "35",
        "Recitation":  "Alafasy_128kbps/069035",
        "Body":  "فَلَيْسَ لَهُ ٱلْيَوْمَ هَٰهُنَا حَمِيمٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "36",
        "Recitation":  "Alafasy_128kbps/069036",
        "Body":  "وَلَا طَعَامٌ إِلَّا مِنْ غِسْلِينٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "37",
        "Recitation":  "Alafasy_128kbps/069037",
        "Body":  "لَّا يَأْكُلُهُۥٓ إِلَّا ٱلْخَٰطِـُٔونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "38",
        "Recitation":  "Alafasy_128kbps/069038",
        "Body":  "فَلَآ أُقْسِمُ بِمَا تُبْصِرُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "39",
        "Recitation":  "Alafasy_128kbps/069039",
        "Body":  "وَمَا لَا تُبْصِرُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "40",
        "Recitation":  "Alafasy_128kbps/069040",
        "Body":  "إِنَّهُۥ لَقَوْلُ رَسُولٍ كَرِيمٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "41",
        "Recitation":  "Alafasy_128kbps/069041",
        "Body":  "وَمَا هُوَ بِقَوْلِ شَاعِرٍ ۚ قَلِيلًا مَّا تُؤْمِنُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "42",
        "Recitation":  "Alafasy_128kbps/069042",
        "Body":  "وَلَا بِقَوْلِ كَاهِنٍ ۚ قَلِيلًا مَّا تَذَكَّرُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "43",
        "Recitation":  "Alafasy_128kbps/069043",
        "Body":  "تَنزِيلٌ مِّن رَّبِّ ٱلْعَٰلَمِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "44",
        "Recitation":  "Alafasy_128kbps/069044",
        "Body":  "وَلَوْ تَقَوَّلَ عَلَيْنَا بَعْضَ ٱلْأَقَاوِيلِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "45",
        "Recitation":  "Alafasy_128kbps/069045",
        "Body":  "لَأَخَذْنَا مِنْهُ بِٱلْيَمِينِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "46",
        "Recitation":  "Alafasy_128kbps/069046",
        "Body":  "ثُمَّ لَقَطَعْنَا مِنْهُ ٱلْوَتِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "47",
        "Recitation":  "Alafasy_128kbps/069047",
        "Body":  "فَمَا مِنكُم مِّنْ أَحَدٍ عَنْهُ حَٰجِزِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "48",
        "Recitation":  "Alafasy_128kbps/069048",
        "Body":  "وَإِنَّهُۥ لَتَذْكِرَةٌ لِّلْمُتَّقِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "49",
        "Recitation":  "Alafasy_128kbps/069049",
        "Body":  "وَإِنَّا لَنَعْلَمُ أَنَّ مِنكُم مُّكَذِّبِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "50",
        "Recitation":  "Alafasy_128kbps/069050",
        "Body":  "وَإِنَّهُۥ لَحَسْرَةٌ عَلَى ٱلْكَٰفِرِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "51",
        "Recitation":  "Alafasy_128kbps/069051",
        "Body":  "وَإِنَّهُۥ لَحَقُّ ٱلْيَقِينِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "69",
        "AyahNumber":  "52",
        "Recitation":  "Alafasy_128kbps/069052",
        "Body":  "فَسَبِّحْ بِٱسْمِ رَبِّكَ ٱلْعَظِيمِ"
    }
]
