[
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "1",
        "Recitation":  "Alafasy_128kbps/044001",
        "Body":  "حمٓ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "2",
        "Recitation":  "Alafasy_128kbps/044002",
        "Body":  "وَٱلْكِتَٰبِ ٱلْمُبِينِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "3",
        "Recitation":  "Alafasy_128kbps/044003",
        "Body":  "إِنَّآ أَنزَلْنَٰهُ فِى لَيْلَةٍ مُّبَٰرَكَةٍ ۚ إِنَّا كُنَّا مُنذِرِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "4",
        "Recitation":  "Alafasy_128kbps/044004",
        "Body":  "فِيهَا يُفْرَقُ كُلُّ أَمْرٍ حَكِيمٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "5",
        "Recitation":  "Alafasy_128kbps/044005",
        "Body":  "أَمْرًا مِّنْ عِندِنَآ ۚ إِنَّا كُنَّا مُرْسِلِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "6",
        "Recitation":  "Alafasy_128kbps/044006",
        "Body":  "رَحْمَةً مِّن رَّبِّكَ ۚ إِنَّهُۥ هُوَ ٱلسَّمِيعُ ٱلْعَلِيمُ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "7",
        "Recitation":  "Alafasy_128kbps/044007",
        "Body":  "رَبِّ ٱلسَّمَٰوَٰتِ وَٱلْأَرْضِ وَمَا بَيْنَهُمَآ ۖ إِن كُنتُم مُّوقِنِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "8",
        "Recitation":  "Alafasy_128kbps/044008",
        "Body":  "لَآ إِلَٰهَ إِلَّا هُوَ يُحْىِۦ وَيُمِيتُ ۖ رَبُّكُمْ وَرَبُّ ءَابَآئِكُمُ ٱلْأَوَّلِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "9",
        "Recitation":  "Alafasy_128kbps/044009",
        "Body":  "بَلْ هُمْ فِى شَكٍّ يَلْعَبُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "10",
        "Recitation":  "Alafasy_128kbps/044010",
        "Body":  "فَٱرْتَقِبْ يَوْمَ تَأْتِى ٱلسَّمَآءُ بِدُخَانٍ مُّبِينٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "11",
        "Recitation":  "Alafasy_128kbps/044011",
        "Body":  "يَغْشَى ٱلنَّاسَ ۖ هَٰذَا عَذَابٌ أَلِيمٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "12",
        "Recitation":  "Alafasy_128kbps/044012",
        "Body":  "رَّبَّنَا ٱكْشِفْ عَنَّا ٱلْعَذَابَ إِنَّا مُؤْمِنُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "13",
        "Recitation":  "Alafasy_128kbps/044013",
        "Body":  "أَنَّىٰ لَهُمُ ٱلذِّكْرَىٰ وَقَدْ جَآءَهُمْ رَسُولٌ مُّبِينٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "14",
        "Recitation":  "Alafasy_128kbps/044014",
        "Body":  "ثُمَّ تَوَلَّوْا۟ عَنْهُ وَقَالُوا۟ مُعَلَّمٌ مَّجْنُونٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "15",
        "Recitation":  "Alafasy_128kbps/044015",
        "Body":  "إِنَّا كَاشِفُوا۟ ٱلْعَذَابِ قَلِيلًا ۚ إِنَّكُمْ عَآئِدُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "16",
        "Recitation":  "Alafasy_128kbps/044016",
        "Body":  "يَوْمَ نَبْطِشُ ٱلْبَطْشَةَ ٱلْكُبْرَىٰٓ إِنَّا مُنتَقِمُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "17",
        "Recitation":  "Alafasy_128kbps/044017",
        "Body":  "۞ وَلَقَدْ فَتَنَّا قَبْلَهُمْ قَوْمَ فِرْعَوْنَ وَجَآءَهُمْ رَسُولٌ كَرِيمٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "18",
        "Recitation":  "Alafasy_128kbps/044018",
        "Body":  "أَنْ أَدُّوٓا۟ إِلَىَّ عِبَادَ ٱللَّهِ ۖ إِنِّى لَكُمْ رَسُولٌ أَمِينٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "19",
        "Recitation":  "Alafasy_128kbps/044019",
        "Body":  "وَأَن لَّا تَعْلُوا۟ عَلَى ٱللَّهِ ۖ إِنِّىٓ ءَاتِيكُم بِسُلْطَٰنٍ مُّبِينٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "20",
        "Recitation":  "Alafasy_128kbps/044020",
        "Body":  "وَإِنِّى عُذْتُ بِرَبِّى وَرَبِّكُمْ أَن تَرْجُمُونِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "21",
        "Recitation":  "Alafasy_128kbps/044021",
        "Body":  "وَإِن لَّمْ تُؤْمِنُوا۟ لِى فَٱعْتَزِلُونِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "22",
        "Recitation":  "Alafasy_128kbps/044022",
        "Body":  "فَدَعَا رَبَّهُۥٓ أَنَّ هَٰٓؤُلَآءِ قَوْمٌ مُّجْرِمُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "23",
        "Recitation":  "Alafasy_128kbps/044023",
        "Body":  "فَأَسْرِ بِعِبَادِى لَيْلًا إِنَّكُم مُّتَّبَعُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "24",
        "Recitation":  "Alafasy_128kbps/044024",
        "Body":  "وَٱتْرُكِ ٱلْبَحْرَ رَهْوًا ۖ إِنَّهُمْ جُندٌ مُّغْرَقُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "25",
        "Recitation":  "Alafasy_128kbps/044025",
        "Body":  "كَمْ تَرَكُوا۟ مِن جَنَّٰتٍ وَعُيُونٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "26",
        "Recitation":  "Alafasy_128kbps/044026",
        "Body":  "وَزُرُوعٍ وَمَقَامٍ كَرِيمٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "27",
        "Recitation":  "Alafasy_128kbps/044027",
        "Body":  "وَنَعْمَةٍ كَانُوا۟ فِيهَا فَٰكِهِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "28",
        "Recitation":  "Alafasy_128kbps/044028",
        "Body":  "كَذَٰلِكَ ۖ وَأَوْرَثْنَٰهَا قَوْمًا ءَاخَرِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "29",
        "Recitation":  "Alafasy_128kbps/044029",
        "Body":  "فَمَا بَكَتْ عَلَيْهِمُ ٱلسَّمَآءُ وَٱلْأَرْضُ وَمَا كَانُوا۟ مُنظَرِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "30",
        "Recitation":  "Alafasy_128kbps/044030",
        "Body":  "وَلَقَدْ نَجَّيْنَا بَنِىٓ إِسْرَٰٓءِيلَ مِنَ ٱلْعَذَابِ ٱلْمُهِينِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "31",
        "Recitation":  "Alafasy_128kbps/044031",
        "Body":  "مِن فِرْعَوْنَ ۚ إِنَّهُۥ كَانَ عَالِيًا مِّنَ ٱلْمُسْرِفِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "32",
        "Recitation":  "Alafasy_128kbps/044032",
        "Body":  "وَلَقَدِ ٱخْتَرْنَٰهُمْ عَلَىٰ عِلْمٍ عَلَى ٱلْعَٰلَمِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "33",
        "Recitation":  "Alafasy_128kbps/044033",
        "Body":  "وَءَاتَيْنَٰهُم مِّنَ ٱلْءَايَٰتِ مَا فِيهِ بَلَٰٓؤٌا۟ مُّبِينٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "34",
        "Recitation":  "Alafasy_128kbps/044034",
        "Body":  "إِنَّ هَٰٓؤُلَآءِ لَيَقُولُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "35",
        "Recitation":  "Alafasy_128kbps/044035",
        "Body":  "إِنْ هِىَ إِلَّا مَوْتَتُنَا ٱلْأُولَىٰ وَمَا نَحْنُ بِمُنشَرِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "36",
        "Recitation":  "Alafasy_128kbps/044036",
        "Body":  "فَأْتُوا۟ بِـَٔابَآئِنَآ إِن كُنتُمْ صَٰدِقِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "37",
        "Recitation":  "Alafasy_128kbps/044037",
        "Body":  "أَهُمْ خَيْرٌ أَمْ قَوْمُ تُبَّعٍ وَٱلَّذِينَ مِن قَبْلِهِمْ ۚ أَهْلَكْنَٰهُمْ ۖ إِنَّهُمْ كَانُوا۟ مُجْرِمِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "38",
        "Recitation":  "Alafasy_128kbps/044038",
        "Body":  "وَمَا خَلَقْنَا ٱلسَّمَٰوَٰتِ وَٱلْأَرْضَ وَمَا بَيْنَهُمَا لَٰعِبِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "39",
        "Recitation":  "Alafasy_128kbps/044039",
        "Body":  "مَا خَلَقْنَٰهُمَآ إِلَّا بِٱلْحَقِّ وَلَٰكِنَّ أَكْثَرَهُمْ لَا يَعْلَمُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "40",
        "Recitation":  "Alafasy_128kbps/044040",
        "Body":  "إِنَّ يَوْمَ ٱلْفَصْلِ مِيقَٰتُهُمْ أَجْمَعِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "41",
        "Recitation":  "Alafasy_128kbps/044041",
        "Body":  "يَوْمَ لَا يُغْنِى مَوْلًى عَن مَّوْلًى شَيْـًٔا وَلَا هُمْ يُنصَرُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "42",
        "Recitation":  "Alafasy_128kbps/044042",
        "Body":  "إِلَّا مَن رَّحِمَ ٱللَّهُ ۚ إِنَّهُۥ هُوَ ٱلْعَزِيزُ ٱلرَّحِيمُ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "43",
        "Recitation":  "Alafasy_128kbps/044043",
        "Body":  "إِنَّ شَجَرَتَ ٱلزَّقُّومِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "44",
        "Recitation":  "Alafasy_128kbps/044044",
        "Body":  "طَعَامُ ٱلْأَثِيمِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "45",
        "Recitation":  "Alafasy_128kbps/044045",
        "Body":  "كَٱلْمُهْلِ يَغْلِى فِى ٱلْبُطُونِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "46",
        "Recitation":  "Alafasy_128kbps/044046",
        "Body":  "كَغَلْىِ ٱلْحَمِيمِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "47",
        "Recitation":  "Alafasy_128kbps/044047",
        "Body":  "خُذُوهُ فَٱعْتِلُوهُ إِلَىٰ سَوَآءِ ٱلْجَحِيمِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "48",
        "Recitation":  "Alafasy_128kbps/044048",
        "Body":  "ثُمَّ صُبُّوا۟ فَوْقَ رَأْسِهِۦ مِنْ عَذَابِ ٱلْحَمِيمِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "49",
        "Recitation":  "Alafasy_128kbps/044049",
        "Body":  "ذُقْ إِنَّكَ أَنتَ ٱلْعَزِيزُ ٱلْكَرِيمُ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "50",
        "Recitation":  "Alafasy_128kbps/044050",
        "Body":  "إِنَّ هَٰذَا مَا كُنتُم بِهِۦ تَمْتَرُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "51",
        "Recitation":  "Alafasy_128kbps/044051",
        "Body":  "إِنَّ ٱلْمُتَّقِينَ فِى مَقَامٍ أَمِينٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "52",
        "Recitation":  "Alafasy_128kbps/044052",
        "Body":  "فِى جَنَّٰتٍ وَعُيُونٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "53",
        "Recitation":  "Alafasy_128kbps/044053",
        "Body":  "يَلْبَسُونَ مِن سُندُسٍ وَإِسْتَبْرَقٍ مُّتَقَٰبِلِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "54",
        "Recitation":  "Alafasy_128kbps/044054",
        "Body":  "كَذَٰلِكَ وَزَوَّجْنَٰهُم بِحُورٍ عِينٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "55",
        "Recitation":  "Alafasy_128kbps/044055",
        "Body":  "يَدْعُونَ فِيهَا بِكُلِّ فَٰكِهَةٍ ءَامِنِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "56",
        "Recitation":  "Alafasy_128kbps/044056",
        "Body":  "لَا يَذُوقُونَ فِيهَا ٱلْمَوْتَ إِلَّا ٱلْمَوْتَةَ ٱلْأُولَىٰ ۖ وَوَقَىٰهُمْ عَذَابَ ٱلْجَحِيمِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "57",
        "Recitation":  "Alafasy_128kbps/044057",
        "Body":  "فَضْلًا مِّن رَّبِّكَ ۚ ذَٰلِكَ هُوَ ٱلْفَوْزُ ٱلْعَظِيمُ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "58",
        "Recitation":  "Alafasy_128kbps/044058",
        "Body":  "فَإِنَّمَا يَسَّرْنَٰهُ بِلِسَانِكَ لَعَلَّهُمْ يَتَذَكَّرُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "44",
        "AyahNumber":  "59",
        "Recitation":  "Alafasy_128kbps/044059",
        "Body":  "فَٱرْتَقِبْ إِنَّهُم مُّرْتَقِبُونَ"
    }
]
