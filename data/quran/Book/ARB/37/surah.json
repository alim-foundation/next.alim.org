        "Recitation":  "Alafasy_128kbps/037006",
        "Body":  "إِنَّا زَيَّنَّا ٱلسَّمَآءَ ٱلدُّنْيَا بِزِينَةٍ ٱلْكَوَاكِبِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "7",
        "Recitation":  "Alafasy_128kbps/037007",
        "Body":  "وَحِفْظًا مِّن كُلِّ شَيْطَٰنٍ مَّارِدٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "8",
        "Recitation":  "Alafasy_128kbps/037008",
        "Body":  "لَّا يَسَّمَّعُونَ إِلَى ٱلْمَلَإِ ٱلْأَعْلَىٰ وَيُقْذَفُونَ مِن كُلِّ جَانِبٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "9",
        "Recitation":  "Alafasy_128kbps/037009",
        "Body":  "دُحُورًا ۖ وَلَهُمْ عَذَابٌ وَاصِبٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "10",
        "Recitation":  "Alafasy_128kbps/037010",
        "Body":  "إِلَّا مَنْ خَطِفَ ٱلْخَطْفَةَ فَأَتْبَعَهُۥ شِهَابٌ ثَاقِبٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "11",
        "Recitation":  "Alafasy_128kbps/037011",
        "Body":  "فَٱسْتَفْتِهِمْ أَهُمْ أَشَدُّ خَلْقًا أَم مَّنْ خَلَقْنَآ ۚ إِنَّا خَلَقْنَٰهُم مِّن طِينٍ لَّازِبٍۭ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "12",
        "Recitation":  "Alafasy_128kbps/037012",
        "Body":  "بَلْ عَجِبْتَ وَيَسْخَرُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "13",
        "Recitation":  "Alafasy_128kbps/037013",
        "Body":  "وَإِذَا ذُكِّرُوا۟ لَا يَذْكُرُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "14",
        "Recitation":  "Alafasy_128kbps/037014",
        "Body":  "وَإِذَا رَأَوْا۟ ءَايَةً يَسْتَسْخِرُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "15",
        "Recitation":  "Alafasy_128kbps/037015",
        "Body":  "وَقَالُوٓا۟ إِنْ هَٰذَآ إِلَّا سِحْرٌ مُّبِينٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "16",
        "Recitation":  "Alafasy_128kbps/037016",
        "Body":  "أَءِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَٰمًا أَءِنَّا لَمَبْعُوثُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "17",
        "Recitation":  "Alafasy_128kbps/037017",
        "Body":  "أَوَءَابَآؤُنَا ٱلْأَوَّلُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "18",
        "Recitation":  "Alafasy_128kbps/037018",
        "Body":  "قُلْ نَعَمْ وَأَنتُمْ دَٰخِرُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "19",
        "Recitation":  "Alafasy_128kbps/037019",
        "Body":  "فَإِنَّمَا هِىَ زَجْرَةٌ وَٰحِدَةٌ فَإِذَا هُمْ يَنظُرُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "20",
        "Recitation":  "Alafasy_128kbps/037020",
        "Body":  "وَقَالُوا۟ يَٰوَيْلَنَا هَٰذَا يَوْمُ ٱلدِّينِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "21",
        "Recitation":  "Alafasy_128kbps/037021",
        "Body":  "هَٰذَا يَوْمُ ٱلْفَصْلِ ٱلَّذِى كُنتُم بِهِۦ تُكَذِّبُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "22",
        "Recitation":  "Alafasy_128kbps/037022",
        "Body":  "۞ ٱحْشُرُوا۟ ٱلَّذِينَ ظَلَمُوا۟ وَأَزْوَٰجَهُمْ وَمَا كَانُوا۟ يَعْبُدُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "23",
        "Recitation":  "Alafasy_128kbps/037023",
        "Body":  "مِن دُونِ ٱللَّهِ فَٱهْدُوهُمْ إِلَىٰ صِرَٰطِ ٱلْجَحِيمِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "24",
        "Recitation":  "Alafasy_128kbps/037024",
        "Body":  "وَقِفُوهُمْ ۖ إِنَّهُم مَّسْـُٔولُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "25",
        "Recitation":  "Alafasy_128kbps/037025",
        "Body":  "مَا لَكُمْ لَا تَنَاصَرُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "26",
        "Recitation":  "Alafasy_128kbps/037026",
        "Body":  "بَلْ هُمُ ٱلْيَوْمَ مُسْتَسْلِمُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "27",
        "Recitation":  "Alafasy_128kbps/037027",
        "Body":  "وَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَسَآءَلُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "28",
        "Recitation":  "Alafasy_128kbps/037028",
        "Body":  "قَالُوٓا۟ إِنَّكُمْ كُنتُمْ تَأْتُونَنَا عَنِ ٱلْيَمِينِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "29",
        "Recitation":  "Alafasy_128kbps/037029",
        "Body":  "قَالُوا۟ بَل لَّمْ تَكُونُوا۟ مُؤْمِنِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "30",
        "Recitation":  "Alafasy_128kbps/037030",
        "Body":  "وَمَا كَانَ لَنَا عَلَيْكُم مِّن سُلْطَٰنٍۭ ۖ بَلْ كُنتُمْ قَوْمًا طَٰغِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "31",
        "Recitation":  "Alafasy_128kbps/037031",
        "Body":  "فَحَقَّ عَلَيْنَا قَوْلُ رَبِّنَآ ۖ إِنَّا لَذَآئِقُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "32",
        "Recitation":  "Alafasy_128kbps/037032",
        "Body":  "فَأَغْوَيْنَٰكُمْ إِنَّا كُنَّا غَٰوِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "33",
        "Recitation":  "Alafasy_128kbps/037033",
        "Body":  "فَإِنَّهُمْ يَوْمَئِذٍ فِى ٱلْعَذَابِ مُشْتَرِكُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "34",
        "Recitation":  "Alafasy_128kbps/037034",
        "Body":  "إِنَّا كَذَٰلِكَ نَفْعَلُ بِٱلْمُجْرِمِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "35",
        "Recitation":  "Alafasy_128kbps/037035",
        "Body":  "إِنَّهُمْ كَانُوٓا۟ إِذَا قِيلَ لَهُمْ لَآ إِلَٰهَ إِلَّا ٱللَّهُ يَسْتَكْبِرُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "36",
        "Recitation":  "Alafasy_128kbps/037036",
        "Body":  "وَيَقُولُونَ أَئِنَّا لَتَارِكُوٓا۟ ءَالِهَتِنَا لِشَاعِرٍ مَّجْنُونٍۭ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "37",
        "Recitation":  "Alafasy_128kbps/037037",
        "Body":  "بَلْ جَآءَ بِٱلْحَقِّ وَصَدَّقَ ٱلْمُرْسَلِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "38",
        "Recitation":  "Alafasy_128kbps/037038",
        "Body":  "إِنَّكُمْ لَذَآئِقُوا۟ ٱلْعَذَابِ ٱلْأَلِيمِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "39",
        "Recitation":  "Alafasy_128kbps/037039",
        "Body":  "وَمَا تُجْزَوْنَ إِلَّا مَا كُنتُمْ تَعْمَلُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "40",
        "Recitation":  "Alafasy_128kbps/037040",
        "Body":  "إِلَّا عِبَادَ ٱللَّهِ ٱلْمُخْلَصِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "41",
        "Recitation":  "Alafasy_128kbps/037041",
        "Body":  "أُو۟لَٰٓئِكَ لَهُمْ رِزْقٌ مَّعْلُومٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "42",
        "Recitation":  "Alafasy_128kbps/037042",
        "Body":  "فَوَٰكِهُ ۖ وَهُم مُّكْرَمُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "43",
        "Recitation":  "Alafasy_128kbps/037043",
        "Body":  "فِى جَنَّٰتِ ٱلنَّعِيمِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "44",
        "Recitation":  "Alafasy_128kbps/037044",
        "Body":  "عَلَىٰ سُرُرٍ مُّتَقَٰبِلِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "45",
        "Recitation":  "Alafasy_128kbps/037045",
        "Body":  "يُطَافُ عَلَيْهِم بِكَأْسٍ مِّن مَّعِينٍۭ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "46",
        "Recitation":  "Alafasy_128kbps/037046",
        "Body":  "بَيْضَآءَ لَذَّةٍ لِّلشَّٰرِبِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "47",
        "Recitation":  "Alafasy_128kbps/037047",
        "Body":  "لَا فِيهَا غَوْلٌ وَلَا هُمْ عَنْهَا يُنزَفُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "48",
        "Recitation":  "Alafasy_128kbps/037048",
        "Body":  "وَعِندَهُمْ قَٰصِرَٰتُ ٱلطَّرْفِ عِينٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "49",
        "Recitation":  "Alafasy_128kbps/037049",
        "Body":  "كَأَنَّهُنَّ بَيْضٌ مَّكْنُونٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "50",
        "Recitation":  "Alafasy_128kbps/037050",
        "Body":  "فَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَسَآءَلُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "51",
        "Recitation":  "Alafasy_128kbps/037051",
        "Body":  "قَالَ قَآئِلٌ مِّنْهُمْ إِنِّى كَانَ لِى قَرِينٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "52",
        "Recitation":  "Alafasy_128kbps/037052",
        "Body":  "يَقُولُ أَءِنَّكَ لَمِنَ ٱلْمُصَدِّقِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "53",
        "Recitation":  "Alafasy_128kbps/037053",
        "Body":  "أَءِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَٰمًا أَءِنَّا لَمَدِينُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "54",
        "Recitation":  "Alafasy_128kbps/037054",
        "Body":  "قَالَ هَلْ أَنتُم مُّطَّلِعُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "55",
        "Recitation":  "Alafasy_128kbps/037055",
        "Body":  "فَٱطَّلَعَ فَرَءَاهُ فِى سَوَآءِ ٱلْجَحِيمِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "56",
        "Recitation":  "Alafasy_128kbps/037056",
        "Body":  "قَالَ تَٱللَّهِ إِن كِدتَّ لَتُرْدِينِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "57",
        "Recitation":  "Alafasy_128kbps/037057",
        "Body":  "وَلَوْلَا نِعْمَةُ رَبِّى لَكُنتُ مِنَ ٱلْمُحْضَرِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "58",
        "Recitation":  "Alafasy_128kbps/037058",
        "Body":  "أَفَمَا نَحْنُ بِمَيِّتِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "59",
        "Recitation":  "Alafasy_128kbps/037059",
        "Body":  "إِلَّا مَوْتَتَنَا ٱلْأُولَىٰ وَمَا نَحْنُ بِمُعَذَّبِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "60",
        "Recitation":  "Alafasy_128kbps/037060",
        "Body":  "إِنَّ هَٰذَا لَهُوَ ٱلْفَوْزُ ٱلْعَظِيمُ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "61",
        "Recitation":  "Alafasy_128kbps/037061",
        "Body":  "لِمِثْلِ هَٰذَا فَلْيَعْمَلِ ٱلْعَٰمِلُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "62",
        "Recitation":  "Alafasy_128kbps/037062",
        "Body":  "أَذَٰلِكَ خَيْرٌ نُّزُلًا أَمْ شَجَرَةُ ٱلزَّقُّومِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "63",
        "Recitation":  "Alafasy_128kbps/037063",
        "Body":  "إِنَّا جَعَلْنَٰهَا فِتْنَةً لِّلظَّٰلِمِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "37",
        "AyahNumber":  "64",
        "Recitation":  "Alafasy_128kbps/037064",
        "Body":  "إِنَّهَا شَجَرَةٌ تَخْرُجُ فِىٓ أَصْلِ ٱلْجَحِيمِ"
    },
    {
        "BookCode":  "ARB",
