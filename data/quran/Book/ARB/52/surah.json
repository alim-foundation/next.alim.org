        "AyahNumber":  "14",
        "Recitation":  "Alafasy_128kbps/052014",
        "Body":  "هَٰذِهِ ٱلنَّارُ ٱلَّتِى كُنتُم بِهَا تُكَذِّبُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "15",
        "Recitation":  "Alafasy_128kbps/052015",
        "Body":  "أَفَسِحْرٌ هَٰذَآ أَمْ أَنتُمْ لَا تُبْصِرُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "16",
        "Recitation":  "Alafasy_128kbps/052016",
        "Body":  "ٱصْلَوْهَا فَٱصْبِرُوٓا۟ أَوْ لَا تَصْبِرُوا۟ سَوَآءٌ عَلَيْكُمْ ۖ إِنَّمَا تُجْزَوْنَ مَا كُنتُمْ تَعْمَلُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "17",
        "Recitation":  "Alafasy_128kbps/052017",
        "Body":  "إِنَّ ٱلْمُتَّقِينَ فِى جَنَّٰتٍ وَنَعِيمٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "18",
        "Recitation":  "Alafasy_128kbps/052018",
        "Body":  "فَٰكِهِينَ بِمَآ ءَاتَىٰهُمْ رَبُّهُمْ وَوَقَىٰهُمْ رَبُّهُمْ عَذَابَ ٱلْجَحِيمِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "19",
        "Recitation":  "Alafasy_128kbps/052019",
        "Body":  "كُلُوا۟ وَٱشْرَبُوا۟ هَنِيٓـًٔۢا بِمَا كُنتُمْ تَعْمَلُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "20",
        "Recitation":  "Alafasy_128kbps/052020",
        "Body":  "مُتَّكِـِٔينَ عَلَىٰ سُرُرٍ مَّصْفُوفَةٍ ۖ وَزَوَّجْنَٰهُم بِحُورٍ عِينٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "21",
        "Recitation":  "Alafasy_128kbps/052021",
        "Body":  "وَٱلَّذِينَ ءَامَنُوا۟ وَٱتَّبَعَتْهُمْ ذُرِّيَّتُهُم بِإِيمَٰنٍ أَلْحَقْنَا بِهِمْ ذُرِّيَّتَهُمْ وَمَآ أَلَتْنَٰهُم مِّنْ عَمَلِهِم مِّن شَىْءٍ ۚ كُلُّ ٱمْرِئٍۭ بِمَا كَسَبَ رَهِينٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "22",
        "Recitation":  "Alafasy_128kbps/052022",
        "Body":  "وَأَمْدَدْنَٰهُم بِفَٰكِهَةٍ وَلَحْمٍ مِّمَّا يَشْتَهُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "23",
        "Recitation":  "Alafasy_128kbps/052023",
        "Body":  "يَتَنَٰزَعُونَ فِيهَا كَأْسًا لَّا لَغْوٌ فِيهَا وَلَا تَأْثِيمٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "24",
        "Recitation":  "Alafasy_128kbps/052024",
        "Body":  "۞ وَيَطُوفُ عَلَيْهِمْ غِلْمَانٌ لَّهُمْ كَأَنَّهُمْ لُؤْلُؤٌ مَّكْنُونٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "25",
        "Recitation":  "Alafasy_128kbps/052025",
        "Body":  "وَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَسَآءَلُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "26",
        "Recitation":  "Alafasy_128kbps/052026",
        "Body":  "قَالُوٓا۟ إِنَّا كُنَّا قَبْلُ فِىٓ أَهْلِنَا مُشْفِقِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "27",
        "Recitation":  "Alafasy_128kbps/052027",
        "Body":  "فَمَنَّ ٱللَّهُ عَلَيْنَا وَوَقَىٰنَا عَذَابَ ٱلسَّمُومِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "28",
        "Recitation":  "Alafasy_128kbps/052028",
        "Body":  "إِنَّا كُنَّا مِن قَبْلُ نَدْعُوهُ ۖ إِنَّهُۥ هُوَ ٱلْبَرُّ ٱلرَّحِيمُ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "29",
        "Recitation":  "Alafasy_128kbps/052029",
        "Body":  "فَذَكِّرْ فَمَآ أَنتَ بِنِعْمَتِ رَبِّكَ بِكَاهِنٍ وَلَا مَجْنُونٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "30",
        "Recitation":  "Alafasy_128kbps/052030",
        "Body":  "أَمْ يَقُولُونَ شَاعِرٌ نَّتَرَبَّصُ بِهِۦ رَيْبَ ٱلْمَنُونِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "31",
        "Recitation":  "Alafasy_128kbps/052031",
        "Body":  "قُلْ تَرَبَّصُوا۟ فَإِنِّى مَعَكُم مِّنَ ٱلْمُتَرَبِّصِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "32",
        "Recitation":  "Alafasy_128kbps/052032",
        "Body":  "أَمْ تَأْمُرُهُمْ أَحْلَٰمُهُم بِهَٰذَآ ۚ أَمْ هُمْ قَوْمٌ طَاغُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "33",
        "Recitation":  "Alafasy_128kbps/052033",
        "Body":  "أَمْ يَقُولُونَ تَقَوَّلَهُۥ ۚ بَل لَّا يُؤْمِنُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "34",
        "Recitation":  "Alafasy_128kbps/052034",
        "Body":  "فَلْيَأْتُوا۟ بِحَدِيثٍ مِّثْلِهِۦٓ إِن كَانُوا۟ صَٰدِقِينَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "35",
        "Recitation":  "Alafasy_128kbps/052035",
        "Body":  "أَمْ خُلِقُوا۟ مِنْ غَيْرِ شَىْءٍ أَمْ هُمُ ٱلْخَٰلِقُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "36",
        "Recitation":  "Alafasy_128kbps/052036",
        "Body":  "أَمْ خَلَقُوا۟ ٱلسَّمَٰوَٰتِ وَٱلْأَرْضَ ۚ بَل لَّا يُوقِنُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "37",
        "Recitation":  "Alafasy_128kbps/052037",
        "Body":  "أَمْ عِندَهُمْ خَزَآئِنُ رَبِّكَ أَمْ هُمُ ٱلْمُصَۣيْطِرُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "38",
        "Recitation":  "Alafasy_128kbps/052038",
        "Body":  "أَمْ لَهُمْ سُلَّمٌ يَسْتَمِعُونَ فِيهِ ۖ فَلْيَأْتِ مُسْتَمِعُهُم بِسُلْطَٰنٍ مُّبِينٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "39",
        "Recitation":  "Alafasy_128kbps/052039",
        "Body":  "أَمْ لَهُ ٱلْبَنَٰتُ وَلَكُمُ ٱلْبَنُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "40",
        "Recitation":  "Alafasy_128kbps/052040",
        "Body":  "أَمْ تَسْـَٔلُهُمْ أَجْرًا فَهُم مِّن مَّغْرَمٍ مُّثْقَلُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "41",
        "Recitation":  "Alafasy_128kbps/052041",
        "Body":  "أَمْ عِندَهُمُ ٱلْغَيْبُ فَهُمْ يَكْتُبُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "42",
        "Recitation":  "Alafasy_128kbps/052042",
        "Body":  "أَمْ يُرِيدُونَ كَيْدًا ۖ فَٱلَّذِينَ كَفَرُوا۟ هُمُ ٱلْمَكِيدُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "43",
        "Recitation":  "Alafasy_128kbps/052043",
        "Body":  "أَمْ لَهُمْ إِلَٰهٌ غَيْرُ ٱللَّهِ ۚ سُبْحَٰنَ ٱللَّهِ عَمَّا يُشْرِكُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "44",
        "Recitation":  "Alafasy_128kbps/052044",
        "Body":  "وَإِن يَرَوْا۟ كِسْفًا مِّنَ ٱلسَّمَآءِ سَاقِطًا يَقُولُوا۟ سَحَابٌ مَّرْكُومٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "45",
        "Recitation":  "Alafasy_128kbps/052045",
        "Body":  "فَذَرْهُمْ حَتَّىٰ يُلَٰقُوا۟ يَوْمَهُمُ ٱلَّذِى فِيهِ يُصْعَقُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "46",
        "Recitation":  "Alafasy_128kbps/052046",
        "Body":  "يَوْمَ لَا يُغْنِى عَنْهُمْ كَيْدُهُمْ شَيْـًٔا وَلَا هُمْ يُنصَرُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "47",
        "Recitation":  "Alafasy_128kbps/052047",
        "Body":  "وَإِنَّ لِلَّذِينَ ظَلَمُوا۟ عَذَابًا دُونَ ذَٰلِكَ وَلَٰكِنَّ أَكْثَرَهُمْ لَا يَعْلَمُونَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "48",
        "Recitation":  "Alafasy_128kbps/052048",
        "Body":  "وَٱصْبِرْ لِحُكْمِ رَبِّكَ فَإِنَّكَ بِأَعْيُنِنَا ۖ وَسَبِّحْ بِحَمْدِ رَبِّكَ حِينَ تَقُومُ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "52",
        "AyahNumber":  "49",
        "Recitation":  "Alafasy_128kbps/052049",
        "Body":  "وَمِنَ ٱلَّيْلِ فَسَبِّحْهُ وَإِدْبَٰرَ ٱلنُّجُومِ"
    }
]
