[
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "1",
        "Recitation":  "Alafasy_128kbps/054001",
        "Body":  "ٱقْتَرَبَتِ ٱلسَّاعَةُ وَٱنشَقَّ ٱلْقَمَرُ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "2",
        "Recitation":  "Alafasy_128kbps/054002",
        "Body":  "وَإِن يَرَوْا۟ ءَايَةً يُعْرِضُوا۟ وَيَقُولُوا۟ سِحْرٌ مُّسْتَمِرٌّ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "3",
        "Recitation":  "Alafasy_128kbps/054003",
        "Body":  "وَكَذَّبُوا۟ وَٱتَّبَعُوٓا۟ أَهْوَآءَهُمْ ۚ وَكُلُّ أَمْرٍ مُّسْتَقِرٌّ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "4",
        "Recitation":  "Alafasy_128kbps/054004",
        "Body":  "وَلَقَدْ جَآءَهُم مِّنَ ٱلْأَنۢبَآءِ مَا فِيهِ مُزْدَجَرٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "5",
        "Recitation":  "Alafasy_128kbps/054005",
        "Body":  "حِكْمَةٌۢ بَٰلِغَةٌ ۖ فَمَا تُغْنِ ٱلنُّذُرُ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "6",
        "Recitation":  "Alafasy_128kbps/054006",
        "Body":  "فَتَوَلَّ عَنْهُمْ ۘ يَوْمَ يَدْعُ ٱلدَّاعِ إِلَىٰ شَىْءٍ نُّكُرٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "7",
        "Recitation":  "Alafasy_128kbps/054007",
        "Body":  "خُشَّعًا أَبْصَٰرُهُمْ يَخْرُجُونَ مِنَ ٱلْأَجْدَاثِ كَأَنَّهُمْ جَرَادٌ مُّنتَشِرٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "8",
        "Recitation":  "Alafasy_128kbps/054008",
        "Body":  "مُّهْطِعِينَ إِلَى ٱلدَّاعِ ۖ يَقُولُ ٱلْكَٰفِرُونَ هَٰذَا يَوْمٌ عَسِرٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "9",
        "Recitation":  "Alafasy_128kbps/054009",
        "Body":  "۞ كَذَّبَتْ قَبْلَهُمْ قَوْمُ نُوحٍ فَكَذَّبُوا۟ عَبْدَنَا وَقَالُوا۟ مَجْنُونٌ وَٱزْدُجِرَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "10",
        "Recitation":  "Alafasy_128kbps/054010",
        "Body":  "فَدَعَا رَبَّهُۥٓ أَنِّى مَغْلُوبٌ فَٱنتَصِرْ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "11",
        "Recitation":  "Alafasy_128kbps/054011",
        "Body":  "فَفَتَحْنَآ أَبْوَٰبَ ٱلسَّمَآءِ بِمَآءٍ مُّنْهَمِرٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "12",
        "Recitation":  "Alafasy_128kbps/054012",
        "Body":  "وَفَجَّرْنَا ٱلْأَرْضَ عُيُونًا فَٱلْتَقَى ٱلْمَآءُ عَلَىٰٓ أَمْرٍ قَدْ قُدِرَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "13",
        "Recitation":  "Alafasy_128kbps/054013",
        "Body":  "وَحَمَلْنَٰهُ عَلَىٰ ذَاتِ أَلْوَٰحٍ وَدُسُرٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "14",
        "Recitation":  "Alafasy_128kbps/054014",
        "Body":  "تَجْرِى بِأَعْيُنِنَا جَزَآءً لِّمَن كَانَ كُفِرَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "15",
        "Recitation":  "Alafasy_128kbps/054015",
        "Body":  "وَلَقَد تَّرَكْنَٰهَآ ءَايَةً فَهَلْ مِن مُّدَّكِرٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "16",
        "Recitation":  "Alafasy_128kbps/054016",
        "Body":  "فَكَيْفَ كَانَ عَذَابِى وَنُذُرِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "17",
        "Recitation":  "Alafasy_128kbps/054017",
        "Body":  "وَلَقَدْ يَسَّرْنَا ٱلْقُرْءَانَ لِلذِّكْرِ فَهَلْ مِن مُّدَّكِرٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "18",
        "Recitation":  "Alafasy_128kbps/054018",
        "Body":  "كَذَّبَتْ عَادٌ فَكَيْفَ كَانَ عَذَابِى وَنُذُرِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "19",
        "Recitation":  "Alafasy_128kbps/054019",
        "Body":  "إِنَّآ أَرْسَلْنَا عَلَيْهِمْ رِيحًا صَرْصَرًا فِى يَوْمِ نَحْسٍ مُّسْتَمِرٍّ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "20",
        "Recitation":  "Alafasy_128kbps/054020",
        "Body":  "تَنزِعُ ٱلنَّاسَ كَأَنَّهُمْ أَعْجَازُ نَخْلٍ مُّنقَعِرٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "21",
        "Recitation":  "Alafasy_128kbps/054021",
        "Body":  "فَكَيْفَ كَانَ عَذَابِى وَنُذُرِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "22",
        "Recitation":  "Alafasy_128kbps/054022",
        "Body":  "وَلَقَدْ يَسَّرْنَا ٱلْقُرْءَانَ لِلذِّكْرِ فَهَلْ مِن مُّدَّكِرٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "23",
        "Recitation":  "Alafasy_128kbps/054023",
        "Body":  "كَذَّبَتْ ثَمُودُ بِٱلنُّذُرِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "24",
        "Recitation":  "Alafasy_128kbps/054024",
        "Body":  "فَقَالُوٓا۟ أَبَشَرًا مِّنَّا وَٰحِدًا نَّتَّبِعُهُۥٓ إِنَّآ إِذًا لَّفِى ضَلَٰلٍ وَسُعُرٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "25",
        "Recitation":  "Alafasy_128kbps/054025",
        "Body":  "أَءُلْقِىَ ٱلذِّكْرُ عَلَيْهِ مِنۢ بَيْنِنَا بَلْ هُوَ كَذَّابٌ أَشِرٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "26",
        "Recitation":  "Alafasy_128kbps/054026",
        "Body":  "سَيَعْلَمُونَ غَدًا مَّنِ ٱلْكَذَّابُ ٱلْأَشِرُ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "27",
        "Recitation":  "Alafasy_128kbps/054027",
        "Body":  "إِنَّا مُرْسِلُوا۟ ٱلنَّاقَةِ فِتْنَةً لَّهُمْ فَٱرْتَقِبْهُمْ وَٱصْطَبِرْ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "28",
        "Recitation":  "Alafasy_128kbps/054028",
        "Body":  "وَنَبِّئْهُمْ أَنَّ ٱلْمَآءَ قِسْمَةٌۢ بَيْنَهُمْ ۖ كُلُّ شِرْبٍ مُّحْتَضَرٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "29",
        "Recitation":  "Alafasy_128kbps/054029",
        "Body":  "فَنَادَوْا۟ صَاحِبَهُمْ فَتَعَاطَىٰ فَعَقَرَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "30",
        "Recitation":  "Alafasy_128kbps/054030",
        "Body":  "فَكَيْفَ كَانَ عَذَابِى وَنُذُرِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "31",
        "Recitation":  "Alafasy_128kbps/054031",
        "Body":  "إِنَّآ أَرْسَلْنَا عَلَيْهِمْ صَيْحَةً وَٰحِدَةً فَكَانُوا۟ كَهَشِيمِ ٱلْمُحْتَظِرِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "32",
        "Recitation":  "Alafasy_128kbps/054032",
        "Body":  "وَلَقَدْ يَسَّرْنَا ٱلْقُرْءَانَ لِلذِّكْرِ فَهَلْ مِن مُّدَّكِرٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "33",
        "Recitation":  "Alafasy_128kbps/054033",
        "Body":  "كَذَّبَتْ قَوْمُ لُوطٍۭ بِٱلنُّذُرِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "34",
        "Recitation":  "Alafasy_128kbps/054034",
        "Body":  "إِنَّآ أَرْسَلْنَا عَلَيْهِمْ حَاصِبًا إِلَّآ ءَالَ لُوطٍ ۖ نَّجَّيْنَٰهُم بِسَحَرٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "35",
        "Recitation":  "Alafasy_128kbps/054035",
        "Body":  "نِّعْمَةً مِّنْ عِندِنَا ۚ كَذَٰلِكَ نَجْزِى مَن شَكَرَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "36",
        "Recitation":  "Alafasy_128kbps/054036",
        "Body":  "وَلَقَدْ أَنذَرَهُم بَطْشَتَنَا فَتَمَارَوْا۟ بِٱلنُّذُرِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "37",
        "Recitation":  "Alafasy_128kbps/054037",
        "Body":  "وَلَقَدْ رَٰوَدُوهُ عَن ضَيْفِهِۦ فَطَمَسْنَآ أَعْيُنَهُمْ فَذُوقُوا۟ عَذَابِى وَنُذُرِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "38",
        "Recitation":  "Alafasy_128kbps/054038",
        "Body":  "وَلَقَدْ صَبَّحَهُم بُكْرَةً عَذَابٌ مُّسْتَقِرٌّ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "39",
        "Recitation":  "Alafasy_128kbps/054039",
        "Body":  "فَذُوقُوا۟ عَذَابِى وَنُذُرِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "40",
        "Recitation":  "Alafasy_128kbps/054040",
        "Body":  "وَلَقَدْ يَسَّرْنَا ٱلْقُرْءَانَ لِلذِّكْرِ فَهَلْ مِن مُّدَّكِرٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "41",
        "Recitation":  "Alafasy_128kbps/054041",
        "Body":  "وَلَقَدْ جَآءَ ءَالَ فِرْعَوْنَ ٱلنُّذُرُ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "42",
        "Recitation":  "Alafasy_128kbps/054042",
        "Body":  "كَذَّبُوا۟ بِـَٔايَٰتِنَا كُلِّهَا فَأَخَذْنَٰهُمْ أَخْذَ عَزِيزٍ مُّقْتَدِرٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "43",
        "Recitation":  "Alafasy_128kbps/054043",
        "Body":  "أَكُفَّارُكُمْ خَيْرٌ مِّنْ أُو۟لَٰٓئِكُمْ أَمْ لَكُم بَرَآءَةٌ فِى ٱلزُّبُرِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "44",
        "Recitation":  "Alafasy_128kbps/054044",
        "Body":  "أَمْ يَقُولُونَ نَحْنُ جَمِيعٌ مُّنتَصِرٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "45",
        "Recitation":  "Alafasy_128kbps/054045",
        "Body":  "سَيُهْزَمُ ٱلْجَمْعُ وَيُوَلُّونَ ٱلدُّبُرَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "46",
        "Recitation":  "Alafasy_128kbps/054046",
        "Body":  "بَلِ ٱلسَّاعَةُ مَوْعِدُهُمْ وَٱلسَّاعَةُ أَدْهَىٰ وَأَمَرُّ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "47",
        "Recitation":  "Alafasy_128kbps/054047",
        "Body":  "إِنَّ ٱلْمُجْرِمِينَ فِى ضَلَٰلٍ وَسُعُرٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "48",
        "Recitation":  "Alafasy_128kbps/054048",
        "Body":  "يَوْمَ يُسْحَبُونَ فِى ٱلنَّارِ عَلَىٰ وُجُوهِهِمْ ذُوقُوا۟ مَسَّ سَقَرَ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "49",
        "Recitation":  "Alafasy_128kbps/054049",
        "Body":  "إِنَّا كُلَّ شَىْءٍ خَلَقْنَٰهُ بِقَدَرٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "50",
        "Recitation":  "Alafasy_128kbps/054050",
        "Body":  "وَمَآ أَمْرُنَآ إِلَّا وَٰحِدَةٌ كَلَمْحٍۭ بِٱلْبَصَرِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "51",
        "Recitation":  "Alafasy_128kbps/054051",
        "Body":  "وَلَقَدْ أَهْلَكْنَآ أَشْيَاعَكُمْ فَهَلْ مِن مُّدَّكِرٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "52",
        "Recitation":  "Alafasy_128kbps/054052",
        "Body":  "وَكُلُّ شَىْءٍ فَعَلُوهُ فِى ٱلزُّبُرِ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "53",
        "Recitation":  "Alafasy_128kbps/054053",
        "Body":  "وَكُلُّ صَغِيرٍ وَكَبِيرٍ مُّسْتَطَرٌ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "54",
        "Recitation":  "Alafasy_128kbps/054054",
        "Body":  "إِنَّ ٱلْمُتَّقِينَ فِى جَنَّٰتٍ وَنَهَرٍ"
    },
    {
        "BookCode":  "ARB",
        "SurahNumber":  "54",
        "AyahNumber":  "55",
        "Recitation":  "Alafasy_128kbps/054055",
        "Body":  "فِى مَقْعَدِ صِدْقٍ عِندَ مَلِيكٍ مُّقْتَدِرٍۭ"
    }
]
